<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>ru/it/lecm/reports-editor/reports-editor</template-type>
    <components>
        <!-- Title -->
        <component>
            <region-id>title</region-id>
            <url>/lecm/base/title-with-filter-label</url>
            <properties>
                <title>page.lecm.reports-editor.report-info</title>
            </properties>
        </component>
        <!-- Menu -->
        <component>
            <region-id>menu</region-id>
            <url>/lecm/reports-editor/menu</url>
            <properties>
                <selected>reportSettings</selected>
            </properties>
        </component>
        <component>
            <region-id>content</region-id>
            <url>/lecm/reports-editor/main</url>
        </component>
        <!-- Report Editor Toolbar -->
        <component>
            <region-id>reports-editor-toolbar</region-id>
            <url>/lecm/reports-editor-toolbar</url>
            <properties>
                <!-- Доступность кнопки -->
                <previousButton>false</previousButton>
                <nextButton>true</nextButton>
                <!-- Настройки перехода на страницу-->
                <nextPage>report-source-edit</nextPage>
            </properties>
        </component>
    </components>
</template-instance>