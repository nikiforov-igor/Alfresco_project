<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
	<bean id="delegation.custom.config" class="org.springframework.extensions.config.ConfigBootstrap" init-method="register">
		<property name="configService" ref="web.config" />
		<property name="configs">
			<list>
				<value>classpath:alfresco/web-extension/ru/it/lecm/delegation/delegation-share-config-custom.xml</value>
				<value>classpath:alfresco/web-extension/ru/it/lecm/secretary/secretary-share-config-custom.xml</value>
				<value>classpath:alfresco/web-extension/ru/it/lecm/deputy/deputy-share-config-custom.xml</value>
			</list>
		</property>
	</bean>

	<bean id="delegationeResourceBundlesWebApp" class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent">
		<property name="resourceBundles">
			<list>
				<value>alfresco.web-extension.ru.it.lecm.delegation.form</value>
				<value>alfresco.web-extension.ru.it.lecm.secretary.form</value>
				<value>alfresco.web-extension.ru.it.lecm.deputy.form</value>
			</list>
		</property>
	</bean>
</beans>
