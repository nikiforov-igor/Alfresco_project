<?xml version="1.0" encoding="UTF-8"?>
<model name="lecm-currency:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>Logic ECM currency dictionaries model</description>
	<author>Logic ECM</author>
	<version>1.0</version>
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<import uri="http://www.it.ru/lecm/dictionary/1.0" prefix="lecm-dic"/>
	</imports>
	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/dictionary/currency/1.0" prefix="lecm-currency"/>
	</namespaces>

	<types>
		<type name="lecm-currency:currency">
			<title>Logic Ecm Dictionary Currency</title>
			<parent>lecm-dic:plane_dictionary_values</parent>
			<properties>
				<property name="lecm-currency:alphabetic-code">
					<type>d:text</type>
					<mandatory>true</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
				<property name="lecm-currency:ml-alphabetic-code">
					<type>d:mltext</type>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>

				<property name="lecm-currency:digital-code">
					<type>d:int</type>
					<mandatory>true</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
			</properties>
		</type>
	</types>
</model>
