                </association>
                <association name="lecm-meetings:holding-invited-members-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>lecm-representative:representative-type</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
				
            </associations>
            <mandatory-aspects>
