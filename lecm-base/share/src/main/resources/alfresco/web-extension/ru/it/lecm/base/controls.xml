			        <param id="showCreateNewButton" localName="" mandatory="false" visible="false">false</param>
	            </control>
	        </field-type>

	        <field-type id="association:lecm-orgstr:employee" localName="Сотрудник">
	            <control id="association:lecm-orgstr:employee-default-control" template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl" localName="Выбор сотрудника">
	                <param id="startLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Структура_x0020_организации_x0020_и_x0020_сотрудников/cm:Организация/cm:Сотрудники</param>
