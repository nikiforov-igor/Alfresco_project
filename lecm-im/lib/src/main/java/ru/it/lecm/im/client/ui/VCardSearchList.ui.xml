<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel>
		<g:FlowPanel styleName='ijab-vcardsearchlist-outer' >
	      <g:FlexTable ui:field='header' styleName='ijab-vcardsearchlist-header' cellSpacing='0' cellPadding='0'/>
	
	      <g:ScrollPanel>
	        <g:FlexTable ui:field='table' styleName='ijab-vcardsearchlist-body' cellSpacing='0' cellPadding='0'/>
	      </g:ScrollPanel>
		</g:FlowPanel>
		<g:FlowPanel ui:field="buttonOuter">
			<g:Button ui:field='addButton'></g:Button>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 