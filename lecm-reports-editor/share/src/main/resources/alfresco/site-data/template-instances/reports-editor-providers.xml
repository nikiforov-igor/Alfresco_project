<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
    <template-type>ru/it/lecm/reports-editor/reports-editor</template-type>
    <components>
        <!-- Title -->
        <component>
            <region-id>title</region-id>
            <url>/lecm/base/title-with-filter-label</url>
            <properties>
                <title>page.lecm.reports-editor.providers</title>
            </properties>
        </component>
        <!-- Toolbar -->
        <component>
            <region-id>toolbar</region-id>
            <url>/lecm/reports-editor/toolbar</url>
            <properties>
                <itemType>lecm-rpeditor:reportProvider</itemType>
                <bubblingLabel>report-providers</bubblingLabel>
                <showSearch>false</showSearch>
                <showCreateBtn>false</showCreateBtn>
            </properties>
        </component>
        <!-- Menu -->
        <component>
            <region-id>menu</region-id>
            <url>/lecm/reports-editor/menu</url>
            <properties>
                <selected>reportProviders</selected>
            </properties>
        </component>
    </components>
</template-instance>