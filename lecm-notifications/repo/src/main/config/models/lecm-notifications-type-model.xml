<?xml version="1.0" encoding="UTF-8"?>

<model name="lecm-notf-type:dictionaryNotificationTypes" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>Document Types Dictionary</description>
	<author>AIvkin</author>
	<published>2012-12-24</published>
	<version>1.0</version>

	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<import uri="http://www.it.ru/lecm/dictionary/1.0" prefix="lecm-dic"/>
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.it.ru/lecm/notifications/types/1.0" prefix="lecm-notf-type"/>
	</namespaces>

	<types>
		<type name="lecm-notf-type:notification-type">
			<title>Notification Type</title>
			<parent>lecm-dic:plane_dictionary_values</parent>
			<properties>
				<property name="lecm-notf-type:code">
					<title>Code</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
				<property name="lecm-notf-type:spring-bean-id">
					<title>Bean ID</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
				<property name="lecm-notf-type:default-select">
					<title>Default select</title>
					<type>d:boolean</type>
					<mandatory>true</mandatory>
					<default>false</default>
				</property>
			</properties>
		</type>
	</types>
</model>