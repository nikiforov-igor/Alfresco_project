<alfresco-config>
	<config evaluator="node-type" condition="lecmBarcode:employeeToPrinterDictionary">
		<forms>
			<form>
				<field-visibility>
					<show id="lecmBarcode:employeeToPrinterDictionaryPrinterName"/>
					<show id="lecmBarcode:employeeToPrinterDictionaryUsePostScript"/>
					<show id="lecmBarcode:employeeToPrinterDictionaryEmployeeAssoc"/>
				</field-visibility>
				<appearance>
					<field id="lecmBarcode:employeeToPrinterDictionaryUsePostScript">
						<control template="/ru/it/lecm/base-share/components/controls/lecm-checkbox-control.ftl"/>
					</field>
				</appearance>
			</form>
		</forms>
	</config>
	<config evaluator="model-type" condition="lecmBarcode:employeeToPrinterDictionary">
		<forms>
			<form>
				<field-visibility>
					<show id="lecmBarcode:employeeToPrinterDictionaryPrinterName"/>
					<show id="lecmBarcode:employeeToPrinterDictionaryUsePostScript"/>
					<show id="lecmBarcode:employeeToPrinterDictionaryEmployeeAssoc"/>
				</field-visibility>
				<appearance>
					<field id="lecmBarcode:employeeToPrinterDictionaryUsePostScript">
						<control template="/ru/it/lecm/base-share/components/controls/lecm-checkbox-control.ftl"/>
					</field>
				</appearance>
			</form>
		</forms>
	</config>

	<config evaluator="node-type" condition="lecmBarcode:printCommandDictionary">
		<forms>
			<form>
				<field-visibility>
					<show id="cm:name"/>
					<show id="lecmBarcode:printCommandDictionaryDocumentType"/>
					<show id="lecmBarcode:printCommandDictionaryPrintCommand"/>
					<show id="lecmBarcode:printCommandDictionaryReportName"/>
				</field-visibility>
				<appearance>
					<field id="lecmBarcode:printCommandDictionaryPrintCommand">
						<control template="/ru/it/lecm/base-share/components/controls/textarea.ftl"/>
					</field>
				</appearance>
			</form>
		</forms>
	</config>
	<config evaluator="model-type" condition="lecmBarcode:printCommandDictionary">
		<forms>
			<form>
				<field-visibility>
					<show id="cm:name"/>
					<show id="lecmBarcode:printCommandDictionaryDocumentType"/>
					<show id="lecmBarcode:printCommandDictionaryPrintCommand"/>
					<show id="lecmBarcode:printCommandDictionaryReportName"/>
				</field-visibility>
				<appearance>
					<field id="lecmBarcode:printCommandDictionaryPrintCommand">
						<control template="/ru/it/lecm/base-share/components/controls/textarea.ftl"/>
					</field>
				</appearance>
			</form>
		</forms>
	</config>
</alfresco-config>
