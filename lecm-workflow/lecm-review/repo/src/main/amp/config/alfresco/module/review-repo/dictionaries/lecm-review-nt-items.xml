<?xml version='1.0' encoding='UTF-8'?>
<items>
    <item name="lecm-dic:Шаблоны уведомлений" type="lecm-dic:dictionary">
        <property name="cm:name"><![CDATA[Шаблоны уведомлений]]></property>
        <property name="lecm-dic:attributeForShow"><![CDATA[cm:name]]></property>
        <property name="lecm-dic:type"><![CDATA[lecm-notification-template:template]]></property>
        <property name="lecm-dic:plane"><![CDATA[true]]></property>
        <property name="lecm-dic:description"><![CDATA[Шаблоны уведомлений]]></property>
        <items>
            <item name="cm:REVIEW_CANCELED" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[REVIEW_CANCELED]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[{#initiator.wrapAsLink()} отозвал документ {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))} с Вашего ознакомления.]]></property>
                <property name="lecm-notification-template:description"><![CDATA[Ознакомление. Отозвано]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:REVIEW_CANCELED_ALL" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[REVIEW_CANCELED_ALL]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[{#initiator.wrapAsLink()} отозвал документ {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))} с ознакомления.]]></property>
                <property name="lecm-notification-template:description"><![CDATA[Ознакомление. Отозвано все]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:REVIEW_NEED" type="lecm-notification-template:template">
                <property name="lecm-notification-template:template">
                    <![CDATA[Вам необходимо ознакомиться с документом {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}. Срок исполнения: {#formatDate("dd.MM.yyyy", #dueDate)}. Инициатор ознакомления: {#eventExecutor.wrapAsLink(#eventExecutor.attribute("lecm-orgstr:employee-short-name"))}]]></property>
                <property name="cm:name"><![CDATA[REVIEW_NEED]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Ознакомление. Необходимо ознакомиться]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:REVIEW_NEED_COMING_SOON" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[REVIEW_NEED_COMING_SOON]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Напоминание: Вам необходимо ознакомиться с документом {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}, срок ознакомления {#formatDate("dd.MM.yyyy", #dueDate)}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Ознакомление. Напоминание об ознакомлении]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:REVIEW_NEED_OVERDUE" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[REVIEW_NEED_OVERDUE]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Внимание: Вы не ознакомились с документом {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}, срок ознакомления {#formatDate("dd.MM.yyyy", #dueDate)}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Ознакомление. Не ознакомились с документом]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:REVIEW_COMING_SOON" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[REVIEW_COMING_SOON]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Напоминание: Вы направили на ознакомление документ {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}, срок ознакомления {#formatDate("dd.MM.yyyy", #dueDate)}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Ознакомление. Вы направили на ознакомление]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:REVIEW_OVERDUE" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[REVIEW_OVERDUE]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Внимание: С документом {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))} не ознакомились в срок {#formatDate("dd.MM.yyyy", #dueDate)}. Следующие сотрудники не ознакомились: {#employees}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Ознакомление. С документом не ознакомились в срок]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:REVIEW_EXCEEDED_DEADLINE" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[REVIEW_EXCEEDED_DEADLINE]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Внимание: Истек срок ознакомления с документом {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}. Инициатор ознакомления {#initiator.wrapAsLink(#initiator.attribute("lecm-orgstr:employee-short-name"))}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Ознакомление. Истек срок ознакомления]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:REVIEW_APPROACHING_DEADLINE" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[REVIEW_APPROACHING_DEADLINE]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Приблежается срок ознакомления с документом {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}. Инициатор ознакомления {#initiator.wrapAsLink(#initiator.attribute("lecm-orgstr:employee-short-name"))}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Ознакомление. Приблежается срок ознакомления]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
        </items>
    </item>
</items>
