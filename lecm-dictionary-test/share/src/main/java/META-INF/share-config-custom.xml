<alfresco-config>
	<config evaluator="node-type" condition="lecm-dic-address:dictionary_values">
		<forms>
			<form>
				<field-visibility>
					<show id="cm:name" force="true"/>
                    <show id="lecm-dic-address:prefix"/>
				</field-visibility>
			</form>
		</forms>
	</config>

	<config evaluator="model-type" condition="lecm-dic-address:dictionary_values">
		<forms>
			<form>
				<field-visibility>
                    <show id="cm:name" force="true"/>
                    <show id="lecm-dic-address:prefix"/>
				</field-visibility>
			</form>

            <form id="datagrid">
                <field-visibility>
                    <show id="cm:name" />
                    <show id="lecm-dic-address:prefix"/>
                </field-visibility>
            </form>
		</forms>
	</config>

    <config evaluator="node-type" condition="lecm-dic-test:dictionary_values">
