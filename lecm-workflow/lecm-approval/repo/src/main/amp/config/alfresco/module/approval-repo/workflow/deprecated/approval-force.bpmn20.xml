<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
			 xmlns:activiti="http://activiti.org/bpmn"
			 typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath"
			 targetNamespace="http://www.it.ru/logicECM/contract/workflow/createOnBasis/1.0">
	<process id="approvalForce" name="Принудительное согласование">
		<startEvent id="startApprovalForce" name="Start" activiti:formKey="lecmTermApproval:forceApproval"></startEvent>
		<sequenceFlow id="flow1" name="" sourceRef="startApprovalForce" targetRef="forceApproval"></sequenceFlow>
		<scriptTask id="forceApproval" name="Contract force" scriptFormat="javascript">
			<extensionElements>
				<activiti:executionListener event="start" class="org.alfresco.repo.workflow.activiti.listener.ScriptExecutionListener">
					<activiti:field name="script">
						<activiti:string>
							approvalWorkflow.terminateApproval(bpm_package, "isApproved", true);
						</activiti:string>
					</activiti:field>
				</activiti:executionListener>
			</extensionElements>
			<script></script>
		</scriptTask>
		<sequenceFlow id="flow2" name="" sourceRef="forceApproval" targetRef="receiveTask"></sequenceFlow>
		<receiveTask id="receiveTask" name="Receive Task"></receiveTask>
		<sequenceFlow id="flow3" name="" sourceRef="receiveTask" targetRef="endApprovalForce"></sequenceFlow>
		<endEvent id="endApprovalForce" name="End"></endEvent>
	</process>
</definitions>
