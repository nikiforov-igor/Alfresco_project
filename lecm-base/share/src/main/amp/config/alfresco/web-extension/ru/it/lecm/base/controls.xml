<alfresco-config>
	<!--Ридер для кастомного корневого элемента типов полей-->
	<plug-ins>
		<element-readers>
			<element-reader element-name="field-types" class="ru.it.lecm.base.formsConfig.elements.fieldsElement.FieldTypesReader"/>
			<element-reader element-name="forms-info" class="ru.it.lecm.base.formsConfig.elements.formsInfoElement.FormsInfoElementReader"/>
		</element-readers>
	</plug-ins>
	<config>
		<field-types>

			<field-type id="cm:cmobject" localName="Базовый объект">
				<control id="cm:object-assoc-tree-control" template="/ru/it/lecm/base-share/components/controls/association-tree-picker.ftl" localName="Выбор ассоциаций из дерева">
					<param id="rootLocation" localName="Корневая директория" mandatory="true" visible="true">/app:company_home/cm:Business_x0020_platform/cm:LECM</param>
					<param id="nameSubstituteString" localName="Шаблон имени" mandatory="true" visible="true">{cm:name}</param>
					<param id="treeNodeSubstituteString" localName="Шаблон имени узла дерева" mandatory="false" visible="true">{cm:name}</param>
					<param id="treeNodeTitleSubstituteString" localName="Шаблон заголовка узла дерева" mandatory="false" visible="true">{cm:name}</param>
					<param id="plane" localName="Плоский" mandatory="false" visible="true">false</param>
					<param id="showSearch" localName="Показывать поиск" mandatory="false" visible="true">false</param>
					<param id="showCreateNewLink" localName="Показывать ссылку создания нового" mandatory="false" visible="true">false</param>
					<param id="showCreateNewButton" localName="Показывать кнопку создания нового" mandatory="false" visible="true">false</param>
					<param id="showSelectedItemsPath" localName="Показывать путь выбранного элемента" mandatory="false" visible="true">false</param>
					<param id="treeItemType" localName="Тип элементов в дереве" mandatory="false" visible="true"/>
				</control>

				<control id="cm:object-assoc-auto-control" template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl" localName="Выбор ассоциаций с автодополнением">
					<param id="startLocation" localName="Начальная директория" mandatory="false" visible="true"/>
					<param id="nameSubstituteString" localName="Шаблон имени" mandatory="false" visible="true"/>
					<param id="showCreateNewLink" localName="Показывать ссылку создания нового" mandatory="false" visible="true"/>
					<param id="showCreateNewButton" localName="Показывать кнопку создания нового" mandatory="false" visible="true"/>
				</control>

				<control id="cm:object-assoc-search-controls" template="/ru/it/lecm/base-share/components/controls/association-search-control.ftl" localName="Контрол поиска">
					<param id="rootLocation" visible="true" localName="Начальная директория"/>
					<param id="nameSubstituteString" visible="true" localName="Шаблон имени"/>
					<param id="selectedItemsNameSubstituteString" visible="true" localName="Шаблон выбраных элементов"/>
					<param id="changeItemsFireAction" visible="true" localName="Событие при изменении элемента"/>
				</control>
				<control id="cm:object-assoc-select-one-control" template="/ru/it/lecm/base-share/components/controls/association-select-one-control.ftl" localName="Выбор ассоциаций из выпадающего списка">
					<param id="startLocation" localName="Начальная директория" mandatory="true" visible="true"/>
					<param id="nameSubstituteString" localName="Шаблон имени" mandatory="false" visible="true"/>
					<param id="showCreateNewButton" localName="Показывать кнопку создания нового" mandatory="false" visible="true"/>
					<param id="notSelectedOptionShow" localName="Допускать пустой выбор" mandatory="false" visible="true"/>
					<param id="notSelectedOptionLabel" localName="Надпись для пустого выбора" mandatory="false" visible="true"/>
				</control>
			</field-type>

			<field-type id="cm:content" localName="Вложения">
				<control id="uploader-with-preview-control" template="/ru/it/lecm/base-share/components/controls/lecm-dnd-uploader-control-with-preview.ftl" localName="Загрузчик файлов">
					<param id="uploadDirectoryPath" localName="Директория загрузки" mandatory="true" visible="true">{usertemp}</param>
					<param id="directoryName" localName="Название директории" mandatory="true" visible="true"/>
				</control>
			</field-type>

			<field-type id="d:text" localName="Текст">
				<control id="d:text-textarea" template="/ru/it/lecm/base-share/components/controls/textarea.ftl" localName="Многострочное текстовое поле">
					<param id="rows" visible="true" localName="Количество строк">2</param>
					<param id="columns" visible="true" localName="Количество столбцов">60</param>
					<param id="defaultValue" visible="true" localName="Значение по умолчанию"/>
					<param id="styleClass" visible="true" localName="Список дополнительных стилей"/>
					<param id="forceEditable" visible="true" localName="Включить принудительное редактирование">false</param>
					<param id="style" visible="true" localName="CSS стиль"/>
					<param id="containerStyleClass" visible="true" localName="Список дополнительных стилей контейнера"/>
				</control>
				<control id="d:text-richtext" template="/ru/it/lecm/base-share/components/controls/richtext.ftl" localName="Многострочное текстовое поле с форматированием">
					<param id="rows" visible="true" localName="Количество строк">2</param>
					<param id="columns" visible="true" localName="Количество столбцов">60</param>
					<param id="styleClass" visible="true" localName="Список дополнительных стилей"/>
					<param id="forceEditable" visible="true" localName="Включить принудительное редактирование">false</param>
					<param id="style" visible="true" localName="CSS стиль"/>
				</control>
				<control id="cm:assoc-auto-text-control" template="/ru/it/lecm/base-share/components/controls/lecm-association-autocomplete-text-control.ftl" localName="Выбор ассоциаций с возможностью ручного ввода">
					<param id="startLocation" visible="true" localName="Корневая папка" mandatory="true" description="Путь к папке, относительно которой получать элементы справочника"/>
					<param id="itemType" visible="true" localName="Тип выбираемых элементов"/>
					<param id="nameSubstituteString" visible="true" localName="Шаблон имени элемента"/>
					<param id="showCreateNewLink" visible="true" localName="Показывать ссылку создания"/>
					<param id="additionalFilter" visible="true" localName="Дополнительный фильтр (FTS query)"/>
					<param id="defaultValueDataSource" visible="true" localName="Скрипт для получения значения по умолчанию"/>
					<param id="childrenDataSource" visible="true" localName="Скрипт для получения списка значений"/>
					<param id="selectedItemsNameSubstituteString" visible="true" localName="Шаблон имени выбранных элементов"/>
					<param id="sortProp" visible="true" localName="Свойство для сортировки значений">cm:name</param>
					<param id="maxSearchResults" visible="true" localName="Максимальное число выводимых элементов">10</param>
				</control>
			</field-type>
			<field-type id="d:date" localName="Дата и время">
				<control id="date-control" template="/ru/it/lecm/base-share/components/controls/date.ftl" localName="Дата/время">
					<param id="showTime" visible="false" localName="Показывать время" description="true - показывать; false - не показывать">false</param>
					<param id="minLimitCurrentDate" visible="true" localName="Не позволять выбирать дату меньше текущей" description="true - нельзя выбирать дату в прошлом; false - можно выбирать любую дату"/>
					<param id="minLimitArg" visible="true" localName="Минимальная дата, доступная для выбора"/>
					<param id="maxLimitCurrentDate" localName="Не позволять выбирать дату больше текущей" description="true - нельзя выбирать дату в будущем; false - можно выбирать любую дату"/>
					<param id="maxLimitArg" visible="true" localName="Максимальная дата, доступная для выбора"/>
					<param id="forceEditable" visible="true" localName="Включить принудительное редактирование">false</param>
					<param id="hideDateFormat" visible="false" localName="Скрывать подсказку с форматом">true</param>
					<param id="dateFormatString" visible="true" localName="Формат выводимой даты"/>
					<param id="defaultValue" visible="true" localName="Значение по умолчанию" description="В формате 'yyyy-MM-dd' или код 'now'"/>
					<param id="defaultScriptURL" visible="true" localName="Скрипт для получения значения по умолчанию"/>
					<param id="changeFireAction" visible="true" localName="Событие, вызываемое при смене значения"/>
				</control>
			</field-type>
			<field-type id="d:datetime" localName="Дата и время">
				<control id="datetime-control" template="/ru/it/lecm/base-share/components/controls/date.ftl" localName="Дата/время">
					<param id="showTime" visible="false" localName="Показывать время" description="true - показывать; false - не показывать">true</param>
					<param id="minLimitCurrentDate" visible="true" localName="Не позволять выбирать дату меньше текущей" description="true - нельзя выбирать дату в прошлом; false - можно выбирать любую дату"/>
					<param id="minLimitArg" visible="true" localName="Минимальная дата, доступная для выбора"/>
					<param id="maxLimitCurrentDate" visible="true" localName="Не позволять выбирать дату больше текущей" description="true - нельзя выбирать дату в будущем; false - можно выбирать любую дату"/>
					<param id="maxLimitArg" visible="true" localName="Максимальная дата, доступная для выбора"/>
					<param id="forceEditable" visible="true" localName="Включить принудительное редактирование">false</param>
					<param id="hideDateFormat" visible="false" localName="Скрывать подсказку с форматом">true</param>
					<param id="dateFormatString" visible="true" localName="Формат выводимой даты"/>
					<param id="defaultValue" visible="true" localName="Значение по умолчанию" description="В формате 'yyyy-MM-dd' или код 'now'"/>
					<param id="defaultScriptURL" visible="true" localName="Скрипт для получения значения по умолчанию"/>
					<param id="changeFireAction" visible="true" localName="Событие, вызываемое при смене значения"/>
				</control>
			</field-type>

			<field-type id="date-range" localName="Временной промежуток">
				<control id="date-range" template="/ru/it/lecm/base-share/components/controls/daterange-control.ftl" localName="Диапазон дат">
					<param id="mandatory" localName="Обязательность заполнения">false</param>
					<param id="forceEditable" localName="Включить принудительное редактирование">false</param>
					<param id="fillDates" localName="Включить предзаполнение дат">false</param>
					<param id="fromDateDefault" localName="Предзаполнить дату (От)"/>
					<param id="toDateDefault" localName="Предзаполнить дату (До)"/>
					<param id="minToLimitCurrentDate" localName="Не позволять выбирать дату меньше текущей (До)" description="true - нельзя выбирать дату в прошлом; false - можно выбирать любую дату"/>
					<param id="minToLimit" localName="Минимальная дата, доступная для выбора (До)"/>
					<param id="minFromLimitCurrentDate" localName="Не позволять выбирать дату меньше текущей (С)" description="true - нельзя выбирать дату в прошлом; false - можно выбирать любую дату"/>
					<param id="minFromLimit" localName="Минимальная дата, доступная для выбора (С)"/>
					<param id="maxToLimitCurrentDate" localName="Не позволять выбирать дату больше текущей (До)" description="true - нельзя выбирать дату в будущем; false - можно выбирать любую дату"/>
					<param id="maxToLimit" localName="Максимальная дата, доступная для выбора (До)"/>
					<param id="maxFromLimitCurrentDate" localName="Не позволять выбирать дату больше текущей (С)" description="true - нельзя выбирать дату в будущем; false - можно выбирать любую дату"/>
					<param id="maxFromLimit" localName="Максимальная дата, доступная для выбора (С)"/>
				</control>
			</field-type>
			<field-type id="number-range" localName="Числовой диапазон">
				<control id="number-range-control" template="/ru/it/lecm/base-share/components/controls/numberrange.ftl" localName="Выбор диапазона чисел">
				</control>
			</field-type>

			<field-type id="int-range" localName="Числовой промежуток (int)">
				<control id="int-range-default-control" template="/ru/it/lecm/base-share/components/controls/numberrange.ftl" localName="Выбор числового промежутка (int)"/>
			</field-type>

			<field-type id="float-range" localName="Числовой промежуток (float)">
				<control id="float-range-default-control" template="/ru/it/lecm/base-share/components/controls/numberrange.ftl" localName="Выбор числового промежутка (float)"/>
			</field-type>

			<field-type id="double-range" localName="Числовой промежуток (double)">
				<control id="double-range-default-control" template="/ru/it/lecm/base-share/components/controls/numberrange.ftl" localName="Выбор числового промежутка (double)"/>
			</field-type>

			<field-type id="long-range" localName="Числовой промежуток (long)">
				<control id="long-range-default-control" template="/ru/it/lecm/base-share/components/controls/numberrange.ftl" localName="Выбор числового промежутка (long)"/>
			</field-type>

			<field-type id="lecm-orgstr:organization-unit" localName="Подразделение">
				<control id="lecm-orgstr:organization-unit-default-control" template="/ru/it/lecm/base-share/components/controls/association-tree-picker.ftl" localName="Выбор орг. единицы">
					<param id="rootLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Структура_x0020_организации_x0020_и_x0020_сотрудников/cm:Организация/cm:Структура/cm:Холдинг</param>
					<param id="nameSubstituteString" localName="" mandatory="false" visible="false">{lecm-orgstr:element-short-name}</param>
					<param id="treeNodeSubstituteString" localName="" mandatory="false" visible="false">{lecm-orgstr:element-short-name}</param>
					<param id="treeNodeTitleSubstituteString" localName="" mandatory="false" visible="false">{cm:name}</param>
					<param id="plane" localName="" mandatory="false" visible="false">false</param>
					<param id="showCreateNewLink" localName="Показывать ссылку создания нового" mandatory="false" visible="true">false</param>
					<param id="showCreateNewButton" localName="Показывать кнопку создания нового" mandatory="false" visible="true">false</param>
					<param id="showSelectedItemsPath" localName="" mandatory="false" visible="false">false</param>
				</control>
				<control id="lecm-orgstr:organization-unit-association-control" template="/ru/it/lecm/base-share/components/controls/association-control.ftl" localName="Выбор орг. единицы (association-control)">
					<param id="rootLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Структура_x0020_организации_x0020_и_x0020_сотрудников/cm:Организация/cm:Структура/cm:Холдинг</param>
					<param id="plane" visible="true" localName="Выводить в виде плоского списка" description="Если true - плоский список, иначе - дерево с выбором элементов">false</param>
					<param id="showCreateNewLink" visible="true" localName="Показывать ссылку создания">false</param>
					<param id="showCreateNewButton" visible="true" localName="Показывать кнопку создания">false</param>
					<param id="showSearch" visible="true" localName="Показывать поле поиска">true</param>
					<param id="showPath" visible="true" localName="Показывать путь у выбранных элементов">true</param>
					<param id="showAutocomplete" visible="true" localName="Включить автодополнение">true</param>
					<param id="endpointType" visible="false" localName="Тип выбираемых элементов">lecm-orgstr:organization-unit</param>
				</control>
			</field-type>

			<field-type id="lecm-orgstr:staff-list" localName="Позиции">
				<control id="lecm-orgstr:staff-list-default-control" template="/ru/it/lecm/base-share/components/controls/association-tree-picker.ftl" localName="Выбор штатной позиции">
					<param id="rootLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Структура_x0020_организации_x0020_и_x0020_сотрудников/cm:Организация/cm:Структура</param>
					<param id="nameSubstituteString" localName="" mandatory="false" visible="false">{lecm-orgstr:element-member-position-assoc/cm:name} {lecm-orgstr:element-member-employee-assoc/lecm-orgstr:employee-link-employee-assoc/cm:name}</param>
					<param id="selectedItemsNameSubstituteString" localName="" mandatory="false" visible="false">{lecm-orgstr:element-member-position-assoc/cm:name} {../lecm-orgstr:element-short-name} {lecm-orgstr:element-member-employee-assoc/lecm-orgstr:employee-link-employee-assoc/cm:name}</param>
					<param id="treeNodeSubstituteString" localName="" mandatory="false" visible="false">{lecm-orgstr:element-short-name}</param>
					<param id="treeNodeTitleSubstituteString" localName="" mandatory="false" visible="false">{cm:name}</param>
					<param id="treeItemType" localName="" mandatory="false" visible="false">lecm-orgstr:organization-unit</param>
					<param id="plane" localName="" mandatory="false" visible="false">false</param>
					<param id="showCreateNewLink" localName="Показывать ссылку создания нового" mandatory="false" visible="true">false</param>
					<param id="showCreateNewButton" localName="Показывать кнопку создания нового" mandatory="false" visible="true">false</param>
					<param id="showSelectedItemsPath" localName="" mandatory="false" visible="false">false</param>
					<param id="showSearch" localName="Показывать поиск" mandatory="false" visible="true">false</param>
				</control>
			</field-type>

			<field-type id="lecm-orgstr:workGroup" localName="Рабочие группы">
				<control id="lecm-orgstr:workGroup-default-control" template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl" localName="Выбор рабочей группы">
					<param id="startLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Структура_x0020_организации_x0020_и_x0020_сотрудников/cm:Организация/cm:Структура</param>
					<param id="nameSubstituteString" localName="" mandatory="false" visible="false">{cm:name}</param>
					<param id="showCreateNewLink" localName="Показывать ссылку создания нового" mandatory="false" visible="true">false</param>
					<param id="showCreateNewButton" localName="Показывать кнопку создания нового" mandatory="false" visible="true">false</param>
				</control>
			</field-type>

			<field-type id="lecm-orgstr:workRole" localName="Рабочие роли">
				<control id="lecm-orgstr:workRole-default-control" template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl" localName="Выбор рабочей роли">
					<param id="startLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Справочники/lecm-dic:Роли_x0020_для_x0020_рабочих_x0020_групп</param>
					<param id="nameSubstituteString" localName="" mandatory="false" visible="false">{cm:name}</param>
					<param id="showCreateNewLink" localName="Показывать ссылку создания нового" mandatory="false" visible="true">false</param>
					<param id="showCreateNewButton" localName="Показывать кнопку создания нового" mandatory="false" visible="true">false</param>
				</control>
			</field-type>

			<field-type id="lecm-orgstr-docType:documentType" localName="Тип документа удостоверяющего личность">
				<control id="lecm-orgstr-docType:documentType-default-control" template="/ru/it/lecm/base-share/components/controls/association-tree-picker.ftl" localName="Выбор типа документа">
					<param id="rootLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Справочники/lecm-dic:Виды_x0020_удостоверений_x0020_личности</param>
