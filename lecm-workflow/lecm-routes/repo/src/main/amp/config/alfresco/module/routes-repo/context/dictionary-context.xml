<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="routes.routesMacrosDicBootstrap"
		  parent="lecmDictionaryBootstrap" depends-on="routes.model.bootstrap, businessJournalLogEventsPolicy" >
		<property name="dictionaries">
			<list>
				<value>alfresco/module/${artifactId}/dictionaries/routes-macros-dictionary.xml</value>
				<value>alfresco/module/${artifactId}/dictionaries/routes-macros-dictionary-items.xml</value>
			</list>
		</property>
	</bean>
</beans>
