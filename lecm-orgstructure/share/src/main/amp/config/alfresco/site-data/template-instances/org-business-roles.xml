<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>ru/it/lecm/orgstructure/org-business-roles</template-type>
	<components>
        <!-- Toolbar -->
        <component>
            <region-id>toolbar</region-id>
            <url>/lecm/orgstructure/toolbar</url>
            <properties>
				<showButtons>false</showButtons>
                <showSearchBlock>true</showSearchBlock>
                <showExSearchBtn>true</showExSearchBtn>
                <bubblingLabel>business-role</bubblingLabel>
            </properties>
        </component>
		<!-- Datagrid -->
		<component>
			<region-id>roles-grid</region-id>
			<url>/lecm/orgstructure/business-roles/datagrid</url>
            <properties>
                <pagination>true</pagination>
                <bubblingLabel>business-role</bubblingLabel>
            </properties>
		</component>
        <component>
            <region-id>forbidden</region-id>
            <url>/lecm/base/forbidden</url>
        </component>
	</components>
</template-instance>
