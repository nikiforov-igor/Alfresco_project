<alfresco-config>
    <!-- Apply custom theme -->
    <config evaluator="string-compare" condition="WebFramework">
        <web-framework>
            <defaults>
                <theme>lecmTheme</theme>
            </defaults>
			<module-deployment>
			   <mode>manual</mode>
			   <enable-auto-deploy-modules>true</enable-auto-deploy-modules>
			</module-deployment>
        </web-framework>
    </config>

</alfresco-config>