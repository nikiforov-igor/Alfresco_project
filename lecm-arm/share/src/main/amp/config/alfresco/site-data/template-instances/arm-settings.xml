<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>ru/it/lecm/arm/arm-settings</template-type>
	<components>
		<component>
			<region-id>left-panel</region-id>
			<url>/lecm/arm/tree</url>
		</component>
		<!-- Title -->
		<component>
			<region-id>title</region-id>
			<url>/lecm/base/title-with-filter-label</url>
			<properties>
				<title>page.title.arm.settings</title>
				<filteredLabel>page.content.filter.label</filteredLabel>
			</properties>
		</component>
		<!-- Toolbar -->
		<component>
			<region-id>toolbar</region-id>
			<url>/lecm/arm/toolbar</url>
		</component>
		<!-- Edit -->
		<component>
			<region-id>main-form</region-id>
			<url>/lecm/arm/editNode</url>
		</component>
        <component>
            <region-id>menu</region-id>
            <url>/lecm/arm/menu</url>
            <properties>
                <onHome>true</onHome>
                <selected>home</selected>
            </properties>
        </component>
	</components>
</template-instance>