            <form>
                <field-visibility>
                    <show id="cm:name" force="true"/>

                    <show id="lecm-dic-test:regOneSelect" force="true"/>

                    <show id="lecm-dic-test:regOneAutocomplete" force="true"/>
                    <show id="lecm-dic-test:regManyAutocomplete" force="true"/>

                    <show id="lecm-dic-test:regOneTree" force="true"/>
                    <show id="lecm-dic-test:regManyTree" force="true"/>

                    <show id="lecm-dic-test:addressOneTree" force="true"/>
                    <show id="lecm-dic-test:addressManyTree" force="true"/>
                </field-visibility>
                <appearance>
                    <field id="lecm-dic-test:regOneSelect">
                        <control template="/ru/it/lecm/base-share/components/controls/association-select-one-control.ftl">
                            <control-param name="startLocation">/app:company_home/lecm-dic:Dictionary/lecm-dic:Регионы</control-param>
                            <control-param name="nameSubstituteString">{lecm-dic-regions:code} - {cm:name}</control-param>
                            <control-param name="notSelectedOptionShow">true</control-param>
                            <control-param name="notSelectedOptionLabel">Empty</control-param>
                            <control-param name="showCreateNewButton">true</control-param>
                        </control>
                    </field>

                    <field id="lecm-dic-test:regOneAutocomplete">
                        <control template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl">
                            <control-param name="startLocation">/app:company_home/lecm-dic:Dictionary/lecm-dic:Регионы</control-param>
                            <control-param name="nameSubstituteString">{lecm-dic-regions:code} - {cm:name}</control-param>
                            <control-param name="showCreateNewLink">false</control-param>
                            <control-param name="showCreateNewButton">false</control-param>
                        </control>
                    </field>
                    <field id="lecm-dic-test:regManyAutocomplete">
                        <control template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl">
                            <control-param name="startLocation">/app:company_home/lecm-dic:Dictionary/lecm-dic:Регионы</control-param>
                            <control-param name="nameSubstituteString">{lecm-dic-regions:code} - {cm:name}</control-param>
	                        <control-param name="showCreateNewLink">true</control-param>
	                        <control-param name="showCreateNewButton">true</control-param>
                        </control>
                    </field>

                    <field id="lecm-dic-test:regOneTree">
                        <control template="/ru/it/lecm/base-share/components/controls/association-tree-picker.ftl">
                            <control-param name="rootLocation">/app:company_home/lecm-dic:Dictionary/lecm-dic:Регионы</control-param>
                            <control-param name="nameSubstituteString">{lecm-dic-regions:code} - {cm:name}</control-param>
                            <control-param name="plane">true</control-param>
	                        <control-param name="showCreateNewLink">true</control-param>
	                        <control-param name="showCreateNewButton">true</control-param>
                        </control>
                    </field>
                    <field id="lecm-dic-test:regManyTree">
                        <control template="/ru/it/lecm/base-share/components/controls/association-tree-picker.ftl">
                            <control-param name="rootLocation">/app:company_home/lecm-dic:Dictionary/lecm-dic:Регионы</control-param>
                            <control-param name="nameSubstituteString">{lecm-dic-regions:code} - {cm:name}</control-param>
                            <control-param name="plane">true</control-param>
	                        <control-param name="showCreateNewLink">false</control-param>
	                        <control-param name="showCreateNewButton">false</control-param>
                        </control>
                    </field>

                    <field id="lecm-dic-test:addressOneTree">
                        <control template="/ru/it/lecm/base-share/components/controls/association-tree-picker.ftl">
                            <control-param name="rootLocation">/app:company_home/lecm-dic:Dictionary/lecm-dic:Адрес</control-param>
                            <control-param name="nameSubstituteString">{lecm-dic-address:prefix} - {cm:name}</control-param>
                            <control-param name="plane">false</control-param>
	                        <control-param name="showCreateNewLink">false</control-param>
	                        <control-param name="showCreateNewButton">false</control-param>
                        </control>
                    </field>
                    <field id="lecm-dic-test:addressManyTree">
                        <control template="/ru/it/lecm/base-share/components/controls/association-tree-picker.ftl">
                            <control-param name="rootLocation">/app:company_home/lecm-dic:Dictionary/lecm-dic:Адрес</control-param>
                            <control-param name="nameSubstituteString">{lecm-dic-address:prefix} - {cm:name}</control-param>
                            <control-param name="plane">false</control-param>
	                        <control-param name="showCreateNewLink">true</control-param>
	                        <control-param name="showCreateNewButton">false</control-param>
                        </control>
                    </field>
                </appearance>
            </form>
        </forms>
    </config>

