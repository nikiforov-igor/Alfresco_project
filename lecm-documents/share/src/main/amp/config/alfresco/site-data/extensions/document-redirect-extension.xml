<extension>
	<modules>
		<module>
			<id>Document redirector</id>
			<evaluator type="default.extensibility.evaluator"/>
			<auto-deploy>true</auto-deploy>
			<components>
				<component>
					<scope>template</scope>
					 <region-id>doclib-custom</region-id>
					<source-id>folder-details</source-id>
					<sub-components>
						<sub-component id="document-redirector">
							<evaluations>
								<evaluation id="document-redirector-evaluation">
									<url>/lecm/documents/redirect?nodeRef={nodeRef}&amp;noRedirect={noRedirect}</url>
								</evaluation>
							</evaluations>
						</sub-component>
					</sub-components>
				</component>
				<component>
					<scope>template</scope>
					 <region-id>doclib-custom</region-id>
					<source-id>document-details</source-id>
					<sub-components>
						<sub-component id="document-redirector">
							<evaluations>
								<evaluation id="document-redirector-evaluation">
									<url>/lecm/documents/redirect?nodeRef={nodeRef}&amp;noRedirect={noRedirect}</url>
								</evaluation>
							</evaluations>
						</sub-component>
					</sub-components>
				</component>
			</components>
		</module>
	</modules>
</extension>
