<?xml version="1.0" encoding="UTF-8"?>

<model name="lecm-br5-semantic-aspects:aspects" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>BR5 Semantic Assist aspects</description>
	<author>SNovikov</author>
	<published>2013-07-16</published>
	<version>1.0</version>

    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
    </imports>

	<namespaces>
		<namespace uri="http://www.it.ru/lecm/br5/semantic/aspects/1.0" prefix="lecm-br5-semantic-aspects"/>
	</namespaces>

	<aspects>
		<!-- Аспект который показывает, надо ли загружать документ в БР5 или нет -->
		<aspect name="lecm-br5-semantic-aspects:br5">
			<properties>
                <property name="lecm-br5-semantic-aspects:loaded">
                    <type>d:boolean</type>
                    <default>false</default>
                </property>
                <property name="lecm-br5-semantic-aspects:version">
                    <type>d:text</type>
                    <default>0</default>
                </property>
                <property name="lecm-br5-semantic-aspects:tags">
                    <type>d:any</type>
                </property>
            </properties>
		</aspect>
	</aspects>

</model>