<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="webscript.ru.it.lecm.sample.SampleWebScript.get" class="ru.it.lecm.sample.SampleWebScript" parent="webscript">
		<property name="repository" ref="repositoryHelper"/>
	</bean>

	<bean id="sampleSessionBean" class="ru.it.lecm.sample.beans.SampleSessionBean"/>

    <bean id="sampleSessionWrapper" class="ru.it.lecm.sample.beans.SampleSessionWrapper" parent="baseJavaScriptExtension">
		<property name="sampleSession" ref="sampleSessionBean"/>
        <property name="extensionName" value="sampleSessionJSExtension"/>
    </bean>


	<!-- A simple class that is initialized by Spring
	<bean id="${groupId}.${artifactId}.exampleBean" class="org.alfresco.demoamp.Demo" init-method="init" />
	-->

	<!-- A simple module component that will be executed once
	<bean id="${artifactId}.exampleComponent" class="org.alfresco.demoamp.DemoComponent" parent="module.baseComponent" >
		<property name="moduleId" value="${pom.artifactId}" />
		<property name="name" value="exampleComponent" />
		<property name="description" value="A demonstration component" />
		<property name="sinceVersion" value="2.0" />
		<property name="appliesFromVersion" value="2.0" />
	</bean>
	-->

</beans>
