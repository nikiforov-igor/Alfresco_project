<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
    <!-- Registration of pentaho forms models -->
    <bean id="${groupId}.${artifactId}.pformsBootstrap" parent="dictionaryModelBootstrap"
          depends-on="dictionaryBootstrap">
        <property name="models">
            <list>
                <value>alfresco/module/${artifactId}/models/pentaho-forms-model.xml</value>
            </list>
        </property>
    </bean>

    <bean id="webscript.ru.it.lecm.forms.pentaho.form.get"
          class="ru.it.lecm.forms.pentahoforms.PentahoFormProducer"
          parent="webscript">
        <property name="repository" ref="repositoryHelper"/>
        <property name="nodeService" ref="NodeService" />
        <property name="contentService" ref="ContentService" />
    </bean>
</beans>
