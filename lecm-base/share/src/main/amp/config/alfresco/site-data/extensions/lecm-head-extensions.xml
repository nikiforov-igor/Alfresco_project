<extension>
	<modules>
		<module>
			<id>Lecm head extension</id>
			<evaluator type="default.extensibility.evaluator"/>
			<auto-deploy>true</auto-deploy>
			<components>
				<component>
					<scope>global</scope>
					<region-id>head-resources</region-id>
					<source-id>global</source-id>
					<sub-components>
						<sub-component id="lecm-head">
							<evaluations>
								<evaluation id="lecm-head-evaluation">
									<url>/lecm/base/head/resources</url>
								</evaluation>
							</evaluations>
						</sub-component>
					</sub-components>
				</component>
			</components>
		</module>
	</modules>
</extension>
