    <types>
        <type name="lecm-meetings-ts:item">
            <title>Agenda item</title>
            <parent>lecm-document:tableDataRow</parent>
            <properties>
				<!--Формулировка-->
                <property name="lecm-meetings-ts:item-name">
                    <type>d:text</type>
					<mandatory>true</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
				<!--Содержательная часть-->
                <property name="lecm-meetings-ts:content">
                    <type>d:text</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
                <property name="lecm-meetings-ts:note">
                    <type>d:text</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
				<!--принятое решение-->
                <property name="lecm-meetings-ts:decision">
                    <type>d:text</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
				<!--Фактическое время начала обсуждения-->
                <property name="lecm-meetings-ts:process-start">
                    <type>d:datetime</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
				<!--Фактическое время окончания обсуждения-->
                <property name="lecm-meetings-ts:process-end">
                    <type>d:datetime</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
				<!--выступили-->
                <property name="lecm-meetings-ts:men-spoke">
                    <type>d:text</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
            </properties>
            <associations>
				<!--Докладчик-->
                <association name="lecm-meetings-ts:reporter-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>lecm-orgstr:employee</class>
                        <mandatory>true</mandatory>
                        <many>false</many>
                    </target>
                </association>
				<!--Содокладчики-->
                <association name="lecm-meetings-ts:coreporter-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>cm:content</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
				<!--Тематика-->
                <association name="lecm-meetings-ts:theme-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>lecm-doc-dic:subjects</class>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </target>
                </association>
				<association name="lecm-meetings-ts:documents-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>lecm-document:base</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
				<association name="lecm-meetings-ts:attachments-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>cm:content</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
        </type>
        <type name="lecm-meetings-ts:itemsTable">
            <title>Agenda</title>
            <parent>lecm-document:tableData</parent>
            <overrides>
                <property name="lecm-document:tableDataRowType">
                    <default>lecm-meetings-ts:item</default>
                </property>
                <property name="lecm-document:pageSize">
                    <default>5</default>
                </property>
            </overrides>
        </type>

        <type name="lecm-meetings-ts:holding-item">
            <title>Holding item</title>
            <parent>lecm-document:tableDataRow</parent>
            <properties>
                <!--Формулировка-->
                <property name="lecm-meetings-ts:holding-item-name">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
                <!--Слушали (Рассмотрение пункта)-->
                <property name="lecm-meetings-ts:holding-listen">
                    <type>d:text</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
                <!--Выступали (Выступили)-->
                <property name="lecm-meetings-ts:holding-speak">
                    <type>d:text</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
                <!--Постановили (Принятое решение)-->
                <property name="lecm-meetings-ts:holding-agreed">
                    <type>d:text</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
                <!--Фактическое время начала обсуждения-->
                <property name="lecm-meetings-ts:holding-start-tine">
                    <type>d:datetime</type>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
            </properties>
            <associations>
                <!--Докладчик-->
                <association name="lecm-meetings-ts:holding-reporter-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>cm:content</class>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </target>
                </association>
                <!--Содокладчики-->
                <association name="lecm-meetings-ts:holding-coreporters-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>cm:content</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
                <!--Тематика-->
                <association name="lecm-meetings-ts:holding-theme-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>lecm-doc-dic:subjects</class>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </target>
                </association>
            </associations>
        </type>
        <type name="lecm-meetings-ts:holding-items-table">
            <title>Agenda</title>
            <parent>lecm-document:tableData</parent>
            <overrides>
                <property name="lecm-document:tableDataRowType">
                    <default>lecm-meetings-ts:holding-item</default>
                </property>
                <property name="lecm-document:pageSize">
                    <default>5</default>
                </property>
            </overrides>
        </type>
    </types> 
    <aspects>
        <aspect name="lecm-meetings-ts:items-table-aspect">
            <parent>lecm-document:tableDataAspect</parent>
            <properties>
                <property name="lecm-meetings-ts:items-assoc-text-content">
                    <type>d:text</type>
                </property>
            </properties>
            <associations>
                <association name="lecm-meetings-ts:items-assoc">
                    <source>
                        <mandatory>true</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>lecm-meetings-ts:itemsTable</class>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </target>
                </association>
            </associations>
        </aspect>
        <aspect name="lecm-meetings-ts:holding-items-table-aspect">
            <parent>lecm-document:tableDataAspect</parent>
            <properties>
                <property name="lecm-meetings-ts:holding-items-assoc-text-content">
                    <type>d:text</type>
                </property>
            </properties>
            <associations>
                <association name="lecm-meetings-ts:holding-items-assoc">
                    <source>
                        <mandatory>true</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>lecm-meetings-ts:holding-items-table</class>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </target>
                </association>
            </associations>
        </aspect>
    </aspects>
</model>