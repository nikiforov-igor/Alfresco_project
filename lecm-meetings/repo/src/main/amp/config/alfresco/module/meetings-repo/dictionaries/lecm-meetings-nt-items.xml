<?xml version='1.0' encoding='UTF-8'?>
<items>
    <item name="lecm-dic:Шаблоны уведомлений" type="lecm-dic:dictionary">
        <property name="cm:name"><![CDATA[Шаблоны уведомлений]]></property>
        <property name="lecm-dic:attributeForShow"><![CDATA[cm:name]]></property>
        <property name="lecm-dic:type"><![CDATA[lecm-notification-template:template]]></property>
        <property name="lecm-dic:plane"><![CDATA[true]]></property>
        <property name="lecm-dic:description"><![CDATA[Шаблоны уведомлений]]></property>
        <items>
            <item name="cm:MEETINGS_TIME_APPROVAL" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[MEETINGS_TIME_APPROVAL]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Необходимо согласовать время проведения совещания {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}, председатель: {#mainObject.getFormatted("{lecm-meetings:chairman-assoc/lecm-orgstr:employee-short-name}")}, время: {#formatDate("dd.MM.yyyy - HH:mm", #mainObject.attribute("lecm-events:from-date"))}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Совещания. Соглсование времени]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:MEETINGS_RESPONSIBLE_FOR_RESOURCES" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[MEETINGS_RESPONSIBLE_FOR_RESOURCES]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Запланированное совещание {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))} требует привлечения ресурсов за которые Вы ответственны. Начало {#formatDate("dd.MM.yyyy в HH:mm", #mainObject.attribute("lecm-events:from-date"))}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Совещания. Требует привлечения ресурсов]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:PROTOCOL_REWORK" type="lecm-notification-template:template">
                <property name="lecm-notification-template:template">
                    <![CDATA[Проект документа {#mainObject.wrapAsLink(#mainObject.getFormatted("{lecm-eds-document:document-type-assoc/cm:name} №{~REGNUM|Не присвоено}"))} направлен Вам на доработку]]></property>
                <property name="cm:name"><![CDATA[PROTOCOL_REWORK]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Протокол. Отправлен на доработку]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:PROTOCOL_REGISTER" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[PROTOCOL_REGISTER]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Документ {#mainObject.wrapAsLink(#mainObject.getFormatted("{lecm-eds-document:document-type-assoc/cm:name} №{~REGNUM|Не присвоено}"))} поступил на регистрацию]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Протокол. Поступил на регистрацию]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:PROTOCOL_POINT_EXECUTOR" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[PROTOCOL_POINT_EXECUTOR]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Вы назначены исполнителем по пункту {#pointNumber} протокола {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Протокол. Исполнитель по пункту]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
            <item name="cm:MEETINGS_INVITE_TO_SITE" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[MEETINGS_INVITE_TO_SITE]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[{#eventExecutor.wrapAsLink(#eventExecutor.attribute("lecm-orgstr:employee-short-name"))} пригласил Вас на сайт <a href="{#url}">{#mainObject.attribute("cm:title")}</a>]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Совещания. Приглашение на сайт]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
                <assocs>
                    <assoc type="lecm-notification-template:template-assoc"
                           xpath="/app:company_home/app:dictionary/app:email_templates/cm:defaultNotificationTemplate.ftl"/>
                </assocs>
            </item>
        </items>
    </item>
</items>