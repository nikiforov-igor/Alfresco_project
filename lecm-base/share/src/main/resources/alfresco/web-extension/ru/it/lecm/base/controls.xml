	            </control>
	        </field-type>

	        <field-type id="association:lecm-orgstr:workRole" localName="Рабочие роли">
	            <control id="association:lecm-orgstr:workRole-default-control" template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl" localName="Выбор рабочей роли">
	                <param id="startLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Справочники/lecm-dic:Роли_x0020_для_x0020_рабочих_x0020_групп</param>
			        <param id="nameSubstituteString" localName="" mandatory="false" visible="false">{cm:name}</param>
			        <param id="showCreateNewLink" localName="" mandatory="false" visible="false">false</param>
			        <param id="showCreateNewButton" localName="" mandatory="false" visible="false">false</param>
	            </control>
	        </field-type>

