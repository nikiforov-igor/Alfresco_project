	        </field-type>

	        <field-type id="association:lecm-orgstr:staff-list" localName="Позиции">
	            <control id="association:lecm-orgstr:staff-list-default-control" template="/ru/it/lecm/base-share/components/controls/association-tree-picker.ftl" localName="Выбор даты и времени">
