<?xml version="1.0" encoding="UTF-8"?>
<model name="lecmBarcode:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>Модель печати штрих-кодов</description>
	<author>Logic ECM</author>
	<version>1.0</version>
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<import uri="http://www.it.ru/lecm/dictionary/1.0" prefix="lecm-dic"/>
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
		<import uri="http://www.it.ru/lecm/org/structure/1.0" prefix="lecm-orgstr"/>
	</imports>
	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/barcode/1.0" prefix="lecmBarcode"/>
	</namespaces>

	<types>
		<type name="lecmBarcode:employeeToPrinterDictionary">
			<parent>lecm-dic:plane_dictionary_values</parent>
			<properties>
				<property name="lecmBarcode:employeeToPrinterDictionaryPrinterName">
					<title>Имя принтера</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
				<property name="lecmBarcode:employeeToPrinterDictionaryUsePostScript">
					<title>Использовать PostScript для принтера</title>
					<type>d:boolean</type>
					<default>false</default>
				</property>
			</properties>
			<associations>
				<association name="lecmBarcode:employeeToPrinterDictionaryEmployeeAssoc">
					<title>Сотрудник</title>
					<source>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>lecm-orgstr:employee</class>
						<mandatory>true</mandatory>
						<many>false</many>
					</target>
				</association>
			</associations>
		</type>

		<type name="lecmBarcode:printCommandDictionary">
			<parent>lecm-dic:plane_dictionary_values</parent>
			<properties>
				<property name="lecmBarcode:printCommandDictionaryDocumentType">
					<title>Тип документа</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
				<property name="lecmBarcode:printCommandDictionaryPrintCommand">
					<title>Команда печати</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
				<property name="lecmBarcode:printCommandDictionaryReportName">
					<title>Название отчета со штрихкодом</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
			</properties>
		</type>
	</types>

	<aspects>
		<aspect name="lecmBarcode:printableDocumentAspect">
			<title>Документы, доступные для печати штрихкодов</title>
		</aspect>
	</aspects>
</model>
