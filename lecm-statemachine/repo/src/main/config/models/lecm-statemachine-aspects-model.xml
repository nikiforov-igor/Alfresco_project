<?xml version="1.0" encoding="UTF-8"?>
<model name="lecm-statemachine-aspects:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>Logic ECM statemachine model</description>
	<author>Logic ECM</author>
	<version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
    </imports>
	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/statemachine/aspects/1.0" prefix="lecm-statemachine-aspects"/>
	</namespaces>
	<aspects>
		<!--Аспект, что процесс является системны (т.е. не логгируется и не отображается в списке процессов)-->
		<aspect name="lecm-statemachine-aspects:is-service-workflow"/>
        <aspect name="lecm-statemachine-aspects:is-draft-aspect">
            <properties>
                <property name="lecm-statemachine-aspects:is-draft">
                    <type>d:boolean</type>
                    <default>false</default>
                </property>
            </properties>
        </aspect>
        <aspect name="lecm-statemachine-aspects:is-final-aspect">
            <properties>
                <property name="lecm-statemachine-aspects:is-final">
                    <type>d:boolean</type>
                    <default>false</default>
                </property>
            </properties>
        </aspect>
	</aspects>
</model>