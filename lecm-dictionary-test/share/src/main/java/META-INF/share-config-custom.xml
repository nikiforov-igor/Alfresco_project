					<show id="lecm-dic-orgstr-test:workGroup" force="true"/>
					<show id="lecm-dic-orgstr-test:employee" force="true"/>
					<show id="lecm-dic-orgstr-test:workRole" force="true"/>
				</field-visibility>
				<appearance>
					<field id="lecm-dic-orgstr-test:position" substituteString="{lecm-orgstr:element-member-position-assoc/cm:name} {../lecm-orgstr:element-short-name} {lecm-orgstr:element-member-employee-assoc/lecm-orgstr:employee-link-employee-assoc/cm:name}"/>
