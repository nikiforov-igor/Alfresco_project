<?xml version="1.0" encoding="UTF-8"?>
<model name="lecm-review-result:review-result-model" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>LECM Review Result Model</description>
	<author>vlevin</author>
	<version>1.0</version>

	<imports>
		<!-- Import Alfresco Dictionary Definitions -->
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
		<!-- Импорт модели данных оргструктуры -->
		<import uri="http://www.it.ru/lecm/org/structure/1.0" prefix="lecm-orgstr"/>
		<!-- Импорт модели данных результатов бизнес-процесса -->
		<import uri="http://www.it.ru/logicECM/model/workflow-result/1.0" prefix="lecm-workflow-result"/>
		<!-- Импорт модели данных процесса ознакомления -->
		<import uri="http://www.it.ru/logicECM/model/review/wokflow/1.0" prefix="lecmReview"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/model/review-result/1.0" prefix="lecm-review-result"/>
	</namespaces>

	<types>
		<type name="lecm-review-result:review-result-item">
			<parent>lecm-workflow-result:workflow-result-item</parent>
			<properties>
				<property name="lecm-review-result:review-result-item-result">
					<type>d:text</type>
					<default>NOT_REVIEWED</default>
					<constraints>
						<constraint ref="lecmReview:reviewTaskResultConstraint" />
					</constraints>
				</property>
			</properties>
		</type>
	</types>


</model>