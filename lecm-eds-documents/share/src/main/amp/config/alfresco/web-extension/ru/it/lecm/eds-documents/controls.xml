<alfresco-config>
	<!--Ридер для кастомного корневого элемента типов полей-->
	<plug-ins>
		<element-readers>
			<element-reader element-name="field-types" class="ru.it.lecm.base.formsConfig.elements.fieldsElement.FieldTypesReader"/>
			<element-reader element-name="forms-info" class="ru.it.lecm.base.formsConfig.elements.formsInfoElement.FormsInfoElementReader"/>
		</element-readers>
	</plug-ins>
	<config>
		<field-types>
			<field-type id="lecm-errands-dic:errand-titles" localName="Заголовки поручений">
				<control id="lecm-errands-dic:errand-titles-default-control" template="/ru/it/lecm/base-share/components/controls/lecm-association-autocomplete-text-control.ftl" localName="Выбор заголовка">
					<param id="startLocation">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Справочники/lecm-dic:Заголовки_x0020_поручений</param>
					<param id="itemType">lecm-errands-dic:errand-titles</param>
					<param id="nameSubstituteString">{cm:name}</param>
					<param id="notSelectedOptionShow">true</param>
					<param id="notSelectedOptionLabel"> </param>
					<param id="showCreateNewLink" visible="true" localName="Показывать ссылку создания нового">false</param>
					<param id="showCreateNewButton" visible="true" localName="Показывать кнопку создания нового">false</param>
				</control>
			</field-type>
			<field-type id="lecm-contract-dic:contract-type" localName="Вид договора">
				<control id="lecm-contract-dic:contract-type-default-control" template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl" localName="Выбор вида договора">
					<param id="startLocation">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Справочники/lecm-dic:Вид_x0020_договора</param>
					<param id="showCreateNewLink" visible="true" localName="Показывать ссылку создания нового">false</param>
					<param id="showCreateNewButton" visible="true" localName="Показывать кнопку создания нового">false</param>
					<param id="nameSubstituteString">{cm:name}</param>
					<param id="itemType">lecm-contract-dic:contract-type</param>
				</control>
			</field-type>
			<field-type id="lecm-contract-dic:contract-documents-type" localName="Вид документа к договору">
				<control id="lecm-contract-dic:contract-documents-type-default-control" template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl" localName="Выбор вида документа к договору">
					<param id="startLocation">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Справочники/lecm-dic:Вид_x0020_документа_x0020_к_x0020_договору</param>
					<param id="showCreateNewLink" visible="true" localName="Показывать ссылку создания нового">false</param>
					<param id="showCreateNewButton" visible="true" localName="Показывать кнопку создания нового">false</param>
					<param id="nameSubstituteString">{cm:name}</param>
					<param id="itemType">lecm-contract-dic:contract-documents-type</param>
				</control>
			</field-type>
		</field-types>
	</config>
</alfresco-config>
