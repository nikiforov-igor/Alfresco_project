<?xml version="1.0" encoding="UTF-8"?>
<items>
    <item name="lecm-dic:Шаблоны уведомлений" type="lecm-dic:dictionary">
        <property name="cm:name"><![CDATA[Шаблоны уведомлений]]></property>
        <property name="lecm-dic:attributeForShow"><![CDATA[cm:name]]></property>
        <property name="lecm-dic:type"><![CDATA[lecm-notification-template:template]]></property>
        <property name="lecm-dic:plane"><![CDATA[true]]></property>
        <property name="lecm-dic:description"><![CDATA[Шаблоны уведомлений]]></property>
        <items>
            <item name="cm:DOCUMENT_REWORK" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[DOCUMENT_REWORK]]></property>
                <property name="lecm-notification-template:template"><![CDATA[Документ {#mainObject.wrapAsLink(#mainObject.getFormatted("{lecm-document:present-string}"))} был возвращен на доработку автором документа.]]></property>
                <property name="lecm-notification-template:description"><![CDATA[Документ. Вернуть на доработку]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:DOCUMENT_REGISTER" type="lecm-notification-template:template">
                <property name="lecm-notification-template:template"><![CDATA[Документ {#mainObject.wrapAsLink(#mainObject.getFormatted("{lecm-document:present-string}"))} поступил Вам на регистрацию.]]></property>
                <property name="cm:name"><![CDATA[DOCUMENT_REGISTER]]></property>
                <property name="lecm-notification-template:description"><![CDATA[Документ. Направить на регистрацию]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:DOCUMENT_APPROACHING_DEADLINE" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[DOCUMENT_APPROACHING_DEADLINE]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Приближается срок исполнения документа {#mainObject.wrapAsLink(#mainObject.getFormatted("{lecm-document:present-string}: {lecm-document:title}"))}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Документ. Приближается срок исполнения]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:DOCUMENT_EXCEEDED_DEADLINE" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[DOCUMENT_EXCEEDED_DEADLINE]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Превышен срок исполнения по документу {#mainObject.wrapAsLink(#mainObject.getFormatted("{lecm-document:present-string}: {lecm-document:title}"))}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Документ. Превышен срок исполнения]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
        </items>
    </item>
</items>
