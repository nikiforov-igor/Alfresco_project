<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean parent="webscript" id="webscript.ru.it.lecm.documents.removal.document.delete"
		  class="ru.it.lecm.documents.removal.webscript.DocumentsRemovalWebscript"
		  p:documentsRemovalService-ref="documentsRemovalService"/>

    <bean id="documentsRemovalScriptService" parent="baseScript" class="ru.it.lecm.documents.removal.webscript.DocumentsRemovalScriptService"
          p:documentsRemovalService-ref="documentsRemovalService"
            p:extensionName="documentsRemovalService"/>
</beans>