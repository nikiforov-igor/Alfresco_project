		<pageFamily>user-profile</pageFamily>
		<container>user-profile</container>
	</properties>
	<components>
		<!-- Title -->
		<component>
			<region-id>title</region-id>
			<url>/lecm/base/title-with-filter-label</url>
			<properties>
				<title>page.user-profile.delegation.title</title>
				<filteredLabel>page.user-profile.filteredLabel</filteredLabel>
			</properties>
		</component>
		<!-- Menu -->
		<component>
			<region-id>menu</region-id>
			<url>/ru/it/lecm/user-profile/menu</url>
			<properties>
				<selected>userProfileDelegation</selected>
			</properties>
		</component>
		<component>
			<region-id>content</region-id>
			<url>/lecm/delegation/opts/content</url>
		</component>
		<component>
			<region-id>forbidden</region-id>
			<url>/lecm/delegation/forbidden</url>
