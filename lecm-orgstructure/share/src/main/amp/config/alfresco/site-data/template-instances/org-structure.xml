<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
    <template-type>ru/it/lecm/orgstructure/org-structure</template-type>
    <components>
        <component>
            <region-id>grid</region-id>
            <url>/lecm/orgstructure/grid</url>
            <properties>
                <pagination>true</pagination>
                <bubblingLabel>orgstructure</bubblingLabel>
                <fullDelete>false</fullDelete>
            </properties>
        </component>
        <component>
            <region-id>tree</region-id>
            <url>/lecm/orgstructure/tree</url>
            <properties>
                <bubblingLabel>orgstructure</bubblingLabel>
                <fullDelete>false</fullDelete>
            </properties>
        </component>
        <!-- Toolbar -->
        <component>
            <region-id>toolbar</region-id>
            <url>/lecm/orgstructure/toolbar</url>
            <properties>
                <showSearchBlock>true</showSearchBlock>
                <showExSearchBtn>true</showExSearchBtn>
                <bubblingLabel>orgstructure</bubblingLabel>
	            <newRowLabel>button.new-unit</newRowLabel>
                <showStructure>true</showStructure>
                <showStructureLabel>button.show_structure</showStructureLabel>
            </properties>
        </component>
        <!-- Title -->
        <component>
            <region-id>title</region-id>
            <url>/lecm/base/title-with-filter-label</url>
            <properties>
                <title>page.orgstructure</title>
                <filteredLabel>page.orgstructure.filter.label</filteredLabel>
            </properties>
        </component>
        <!-- Menu -->
        <component>
            <region-id>menu</region-id>
            <url>/lecm/orgstructure/menu</url>
            <properties>
                <selected>orgstructure</selected>
            </properties>
        </component>
        <component>
            <region-id>forbidden</region-id>
            <url>/lecm/base/forbidden</url>
        </component>
    </components>
</template-instance>