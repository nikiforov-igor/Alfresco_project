<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
    <template-type>ru/it/lecm/contracts/contracts-documents</template-type>
    <components>
        <!-- Title -->
        <component>
            <region-id>title</region-id>
            <url>/lecm/base/title-with-filter-label</url>
            <properties>
                <title>page.contracts-documents</title>
                <filteredLabel>page.contracts-documents.filter</filteredLabel>
            </properties>
        </component>
        <!-- Toolbar -->
        <component>
            <region-id>toolbar</region-id>
            <url>/lecm/contracts/toolbar</url>
            <properties>
                <showCreateBtn>false</showCreateBtn>
                <bubblingLabel>documents</bubblingLabel>
            </properties>
        </component>
        <!-- Menu -->
        <component>
            <region-id>menu</region-id>
            <url>/lecm/contracts/menu</url>
            <properties>
                <selected>documents</selected>
            </properties>
        </component>
        <!-- Datagrid -->
        <component>
            <region-id>contracts-documents-grid</region-id>
            <url>/lecm/contracts/contracts-docs-datagrid</url>
            <properties>
                <pagination>true</pagination>
                <bubblingLabel>contracts-documents</bubblingLabel>
                <advSearchFormId>contracts-documents-search</advSearchFormId>
            </properties>
        </component>
        <!-- Filter -->
        <!--<component>
            <region-id>contracts-documents-filter</region-id>
            <url>/lecm/contracts/contracts-filter</url>
            <properties>
                <type>lecm-contract:additionalDocument</type>
                <showStatusesBlock>false</showStatusesBlock>
            </properties>
        </component>-->
        <!-- Forbidden -->
        <component>
            <region-id>forbidden</region-id>
            <url>/lecm/base/forbidden</url>
        </component>
    </components>
</template-instance>