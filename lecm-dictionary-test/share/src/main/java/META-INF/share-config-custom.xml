	<config evaluator="node-type" condition="lecm-dic-orgstr-test:dictionary_values">
		<forms>
			<form>
				<field-visibility>
					<show id="cm:name" force="true"/>

					<show id="lecm-dic-orgstr-test:organization-unit" force="true"/>
					<show id="lecm-dic-orgstr-test:position" force="true"/>
					<show id="lecm-dic-orgstr-test:workGroup" force="true"/>
					<show id="lecm-dic-orgstr-test:employee" force="true"/>
					<show id="lecm-dic-orgstr-test:workRole" force="true"/>
				</field-visibility>
			</form>
		</forms>
	</config>

	<config evaluator="model-type" condition="lecm-dic-orgstr-test:dictionary_values">
		<forms>
			<form>
				<field-visibility>
					<show id="cm:name" force="true"/>

					<show id="lecm-dic-orgstr-test:organization-unit" force="true"/>
					<show id="lecm-dic-orgstr-test:position" force="true"/>
					<show id="lecm-dic-orgstr-test:workGroup" force="true"/>
