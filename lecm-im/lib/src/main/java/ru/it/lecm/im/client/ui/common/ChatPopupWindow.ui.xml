<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>

    </ui:style>
    <g:FlowPanel styleName="ijab-chat-window" >
        <g:HTMLPanel styleName="ijab-chat-window-header">
            <span class="ijab-chat-window-actions">
                <a class="ijab-window-close" ui:field="closeElement" title="Close">
                    <em class="ui-icon ui-icon-close" ui:field="closePicElement">Close</em>
                </a>
            </span>
            <h4 ui:field="captionElement"></h4>
        </g:HTMLPanel>
        <g:FlowPanel styleName="ijab-chat-window-content" ui:field="contentWrap">
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>