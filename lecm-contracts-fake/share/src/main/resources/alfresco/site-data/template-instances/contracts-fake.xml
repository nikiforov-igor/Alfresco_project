<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
    <template-type>ru/it/lecm/documents/documents-list</template-type>
    <properties>
        <docType>lecm-contract-fake:document</docType>
    </properties>
    <components>
        <!-- Title -->
        <component>
            <region-id>title</region-id>
            <url>/lecm/base/title-with-filter-label</url>
            <properties>
                <itemType>lecm-contract-fake:document</itemType>
                <filteredLabel>page.contracts.fake.filter.label</filteredLabel>
            </properties>
        </component>
        <!-- Toolbar -->
        <component>
            <region-id>toolbar</region-id>
            <url>/lecm/documents/documents-list-toolbar</url>
            <properties>
                <itemType>lecm-contract-fake:document</itemType>
                <showCreateBtn>true</showCreateBtn>
                <bubblingLabel>contracts-fake</bubblingLabel>
                <showExSearchBtn>true</showExSearchBtn>
            </properties>
        </component>
        <!-- Menu -->
        <component>
            <region-id>menu</region-id>
            <url>/lecm/contracts/fake/menu</url>
            <properties>
                <selected>contracts-fake</selected>
            </properties>
        </component>
        <!-- Datagrid -->
        <component>
            <region-id>documents-grid</region-id>
            <url>/lecm/documents/documents-list-grid</url>
            <properties>
                <itemType>lecm-contract-fake:document</itemType>
                <bubblingLabel>contracts-fake</bubblingLabel>
                <attributeForShow>lecm-contract:regNumProject</attributeForShow>
                <filterProperty>lecm-statemachine:status</filterProperty>
                <formId>{formId}</formId>
                <filter>{query}</filter>
            </properties>
        </component>
        <!-- Filter -->
        <component>
            <region-id>documents-filter</region-id>
            <url>/lecm/documents/documents-list-filter</url>
            <properties>
                <itemType>lecm-contract-fake:document</itemType>
                <linkPage>contracts-fake</linkPage>
                <queryFilter>docAuthor</queryFilter>
            </properties>
        </component>
        <!-- Forbidden -->
        <component>
            <region-id>forbidden</region-id>
            <url>/lecm/base/forbidden</url>
        </component>
    </components>
</template-instance>