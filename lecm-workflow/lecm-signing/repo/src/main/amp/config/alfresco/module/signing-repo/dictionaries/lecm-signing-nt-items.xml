<?xml version='1.0' encoding='UTF-8'?>
<items>
    <item name="lecm-dic:Шаблоны уведомлений" type="lecm-dic:dictionary">
        <property name="cm:name"><![CDATA[Шаблоны уведомлений]]></property>
        <property name="lecm-dic:attributeForShow"><![CDATA[cm:name]]></property>
        <property name="lecm-dic:type"><![CDATA[lecm-notification-template:template]]></property>
        <property name="lecm-dic:plane"><![CDATA[true]]></property>
        <property name="lecm-dic:description"><![CDATA[Шаблоны уведомлений]]></property>
        <items>
            <item name="cm:SIGNING_DIRECT" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[SIGNING_DIRECT]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[{#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:ext-present-string"))} направлен Вам на подписание.]]></property>
                <property name="lecm-notification-template:description"><![CDATA[Подписание. Направить]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:SIGNING_RESOLUTION" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[SIGNING_RESOLUTION]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Сотрудник {#signer.wrapAsLink(#signer.attribute("lecm-orgstr:employee-short-name"))} {#isSigned ? "подписал документ" : #wrapTitle("отклонил подписание", #comment) + " документа"} {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:ext-present-string"))}]]></property>
                <property name="lecm-notification-template:description"><![CDATA[Подписание. Решение]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:SIGNING_PROJECT_DECISION" type="lecm-notification-template:template">
                <property name="lecm-notification-template:template">
                    <![CDATA[Сотрудник {#eventExecutor.wrapAsLink(#eventExecutor.attribute("lecm-orgstr:employee-short-name"))} {#isSigned ? "подписал проект" : "отклонил подписание проекта"} документа {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}]]></property>
                <property name="cm:name"><![CDATA[SIGNING_PROJECT_DECISION]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Подписание. Решение по проекту]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:SIGNING_NEED_COMING_SOON" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[SIGNING_NEED_COMING_SOON]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Напоминание: Вам необходимо подписать проект документа {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}, срок подписания {#formatDate("dd.MM.yyyy", #dueDate)}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Подписание. Напоминание об подписании]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:SIGNING_NEED_OVERDUE" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[SIGNING_NEED_OVERDUE]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Внимание: Вы не подписали документ {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}, срок подписания {#formatDate("dd.MM.yyyy", #dueDate)}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Подписание. Вы не подписали документ]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:SIGNING_COMING_SOON" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[SIGNING_COMING_SOON]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Напоминание: Вы направили на подписание проект документа {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))}, срок подписания {#formatDate("dd.MM.yyyy", #dueDate)}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Подписание. Вы направили на подписание]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
            <item name="cm:SIGNING_OVERDUE" type="lecm-notification-template:template">
                <property name="cm:name"><![CDATA[SIGNING_OVERDUE]]></property>
                <property name="lecm-notification-template:template">
                    <![CDATA[Внимание: Проект документа {#mainObject.wrapAsLink(#mainObject.attribute("lecm-document:present-string"))} не подписан в срок {#formatDate("dd.MM.yyyy", #dueDate)}. Следующие сотрудники не приняли решение: {#employees}]]></property>
                <property name="lecm-notification-template:description">
                    <![CDATA[Подписание. Документ не подписан в срок]]></property>
                <property name="lecm-notification-template:subject"><![CDATA[]]></property>
            </item>
        </items>
    </item>
</items>