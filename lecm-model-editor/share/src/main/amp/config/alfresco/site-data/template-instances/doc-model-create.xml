<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>ru/it/lecm/docmodel/doc-model-create</template-type>
	<components>

		<!-- Title -->
		<component>
			<region-id>title</region-id>
			<url>/components/title/simple-title</url>
			<properties>
				<title>Редактор моделей</title>
			</properties>
		</component>

		<!-- Menu -->
		<component>
			<region-id>menu</region-id>
			<url>/lecm/documentmodel/menu</url>
		</component>

		<!-- Create Content Form Manager -->
		<component>
			<region-id>create-content-mgr</region-id>
			<url>/lecm/documentmodel/form-mgr/doc-model-create-mgr</url>
		</component>

		<!-- Create Content Form -->
		<component>
			<region-id>create-content</region-id>
			<url>/components/form?mimeType={mimeType}&amp;destination={destination}&amp;formId={formId}&amp;redirect={redirect}</url>
			<properties>
				<itemKind>type</itemKind>
				<itemId>{itemId}</itemId>
				<mode>create</mode>
				<submitType>json</submitType>
				<showCaption>true</showCaption>
				<showCancelButton>true</showCancelButton>
				<editInline>true</editInline>
				<googleEditable>true</googleEditable>
			</properties>
		</component>

	</components>
</template-instance>