<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean init-method="init"
		  depends-on="workflow.model.bootstrap"
		  class="ru.it.lecm.workflow.policies.WorkflowAssigneesListItemPolicy"
		  p:policyComponent-ref="policyComponent"
		  p:nodeService-ref="nodeService"
		  p:orgstructureBean-ref="serviceOrgstructure"/>

	<bean init-method="init"
		  depends-on="workflow.model.bootstrap"
		  class="ru.it.lecm.workflow.policies.RouteRolicy"
		  p:policyComponent-ref="policyComponent"
		  p:nodeService-ref="nodeService"/>

	<bean init-method="init"
		  depends-on="workflow.model.bootstrap"
		  class="ru.it.lecm.workflow.policies.WorkflowResultListPolicy"
		  p:policyComponent-ref="policyComponent"
		  p:nodeService-ref="nodeService"
		  p:orgstructureBean-ref="serviceOrgstructure"/>

	<bean init-method="init"
		  depends-on="workflow.model.bootstrap"
		  class="ru.it.lecm.workflow.policies.WorkflowAssociationPolicy"
		  parent="logicEcmAssociationPolicy"/>

</beans>
