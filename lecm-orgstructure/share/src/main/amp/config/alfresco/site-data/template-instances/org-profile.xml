<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
    <template-type>ru/it/lecm/orgstructure/org-profile</template-type>
    <components>
        <component>
            <region-id>organization-profile</region-id>
            <url>/lecm/orgstructure/organization</url>
        </component>
        <!-- Toolbar -->
        <component>
            <region-id>toolbar</region-id>
            <url>/lecm/orgstructure/toolbar</url>
            <properties>
                <showSearchBlock>false</showSearchBlock>
                <showExSearchBtn>false</showExSearchBtn>
                <showButtons>false</showButtons>
            </properties>
        </component>
        <!-- Title -->
        <component>
            <region-id>title</region-id>
            <url>/lecm/base/title-with-filter-label</url>
            <properties>
                <title>page.organization</title>
                <filteredLabel>page.orgstructure.filter.label</filteredLabel>
            </properties>
        </component>
        <!-- Menu -->
        <component>
            <region-id>menu</region-id>
            <url>/lecm/orgstructure/menu</url>
            <properties>
                <selected>organization</selected>
            </properties>
        </component>
    </components>
</template-instance>
