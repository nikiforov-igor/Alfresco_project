<?xml version="1.0" encoding="UTF-8"?>
<model name="lecmSignResult:signingWorkflowResult" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>Workflow result model for sequential signing</description>
	<author>vmalygin</author>
	<version>1.0</version>

	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<import uri="http://www.it.ru/logicECM/model/workflow-result/1.0" prefix="lecm-workflow-result"/>
		<import uri="http://www.it.ru/logicECM/model/signing/wokflow/1.0" prefix="lecmSign"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/model/signing/workflow/result/1.0" prefix="lecmSignResult"/>
	</namespaces>

	<types>
		<type name="lecmSignResult:signResultList">
			<parent>lecm-workflow-result:workflow-result-list</parent>
			<properties>
				<property name="lecmSignResult:signResultListDecision">
					<type>d:text</type>
					<default>NO_DECISION</default>
					<constraints>
						<constraint ref="lecmSign:signTaskResultConstraint" />
					</constraints>
				</property>
			</properties>
		</type>

		<type name="lecmSignResult:signResultItem">
			<parent>lecm-workflow-result:workflow-result-item</parent>
			<properties>
				<property name="lecmSignResult:signResultItemDecision">
					<type>d:text</type>
					<default>NO_DECISION</default>
					<constraints>
						<constraint ref="lecmSign:signTaskResultConstraint" />
					</constraints>
				</property>
			</properties>
		</type>
	</types>
</model>
