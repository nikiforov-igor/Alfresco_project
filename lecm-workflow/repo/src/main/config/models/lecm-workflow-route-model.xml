<?xml version="1.0" encoding="UTF-8"?>
<model name="lecmRoute:routingWorkflow" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>Workflow model for document routing</description>
	<author>vmalygin</author>
	<version>1.0</version>

	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<import uri="http://www.alfresco.org/model/bpm/1.0" prefix="bpm"/>
		<import uri="http://www.it.ru/logicECM/statemachine/aspects/1.0" prefix="lecm-statemachine-aspects"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/model/routing/wokflow/1.0" prefix="lecmRoute"/>
	</namespaces>

	<types>
		<type name="lecmRoute:routing">
			<parent>bpm:startTask</parent>
			<mandatory-aspects>
				<aspect>lecm-statemachine-aspects:is-service-workflow</aspect>
			</mandatory-aspects>
		</type>
	</types>
</model>
