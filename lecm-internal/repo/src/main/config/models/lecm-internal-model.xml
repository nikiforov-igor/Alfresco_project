	<imports>
		<import uri="http://www.it.ru/logicECM/document/1.0" prefix="lecm-document"/>
		<import uri="http://www.it.ru/logicECM/eds-document/1.0" prefix="lecm-eds-document"/>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <import uri="http://www.it.ru/lecm/document/aspects/1.0" prefix="lecm-document-aspects"/>
        <import uri="http://www.it.ru/lecm/org/structure/1.0" prefix="lecm-orgstr"/>
        <import uri="http://www.it.ru/logicECM/links/1.0" prefix="lecm-links"/>
        <import uri="http://www.it.ru/logicECM/document/dictionaries/1.0" prefix="lecm-doc-dic"/>
        <import uri="http://www.it.ru/logicECM/document/dictionaries/deliveryMethod/1.0" prefix="lecm-doc-dic-dm"/>
        <import uri="http://www.it.ru/logicECM/document/dictionaries/docType/1.0" prefix="lecm-doc-dic-dt"/>
        <import uri="http://www.it.ru/logicECM/document/dictionaries/fileRegister/1.0" prefix="lecm-doc-dic-fr"/>
		<import uri="http://www.it.ru/lecm/contractors/model/contractor/1.0" prefix="lecm-contractor"/>
		<import uri="http://www.it.ru/lecm/contractors/model/representative/1.0" prefix="lecm-representative"/>
	</imports>
	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/internal/1.0" prefix="lecm-internal"/>
	</namespaces>

    <constraints>
        <constraint name="lecm-internal:present-string-constraint" type="ru.it.lecm.documents.constraints.PresentStringConstraint">
            <parameter name="presentString">
                <value><![CDATA[Внутренний от {cm:created?dd-MM-yyyy}, номер {~REGNUM}]]></value>
            </parameter>
        </constraint>
        <constraint name="lecm-internal:attachment-categories" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>Внутренее</value>
                    <value>Прочее</value>
                </list>
            </parameter>
        </constraint>
        <constraint name="lecm-internal:author-property-constraint" type="ru.it.lecm.documents.constraints.AuthorPropertyConstraint">
            <parameter name="authorProperty">
                <value>lecm-document:creator-ref</value>
            </parameter>
        </constraint>
	    <constraint name="lecm-internal:arm-url-constraint" type="ru.it.lecm.documents.constraints.ArmUrlConstraint">
		    <parameter name="armUrl">
			    <value>documents-list?doctype=lecm-internal:document</value>
		    </parameter>
	    </constraint>
    </constraints>

    <types>
		<type name="lecm-internal:document">
			<title>Внутренний</title>
			<parent>lecm-eds-document:base</parent>
		</type>
	</types>
