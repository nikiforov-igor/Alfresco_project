<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
    <template-type>ru/it/lecm/contracts/contracts-list</template-type>
    <components>
        <!-- Title -->
        <component>
            <region-id>title</region-id>
            <url>/lecm/base/title-with-filter-label</url>
            <properties>
                <title>page.contracts</title>
                <filteredLabel>page.contracts.filter</filteredLabel>
            </properties>
        </component>
        <!-- Toolbar -->
        <component>
            <region-id>toolbar</region-id>
            <url>/lecm/contracts/toolbar</url>
            <properties>
                <showCreateBtn>true</showCreateBtn>
                <bubblingLabel>contracts</bubblingLabel>
            </properties>
        </component>
        <!-- Menu -->
        <component>
            <region-id>menu</region-id>
            <url>/lecm/contracts/menu</url>
            <properties>
                <selected>list</selected>
            </properties>
        </component>
        <!-- Datagrid -->
        <component>
            <region-id>contracts-grid</region-id>
            <url>/lecm/contracts/contracts-datagrid</url>
            <properties>
                <pagination>true</pagination>
                <bubblingLabel>contracts</bubblingLabel>
                <advSearchFormId>contracts-search</advSearchFormId>
            </properties>
        </component>
        <!-- Filter -->
        <component>
            <region-id>contracts-filter</region-id>
            <url>/lecm/contracts/contracts-filter</url>
            <properties>
                <type>lecm-contract:document</type>
                <showStatusesBlock>true</showStatusesBlock>
            </properties>
        </component>
        <!-- Forbidden -->
        <component>
            <region-id>forbidden</region-id>
            <url>/lecm/base/forbidden</url>
        </component>
    </components>
</template-instance>