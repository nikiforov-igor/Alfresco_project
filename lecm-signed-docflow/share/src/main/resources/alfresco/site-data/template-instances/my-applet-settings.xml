<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>ru/it/lecm/signed/docflow/my-applet-settings</template-type>
	<properties>
		<pageFamily>user-profile</pageFamily>
		<container>user-profile</container>
	</properties>
	<components>
		<!-- Title -->
		<component>
			<region-id>title</region-id>
			<url>/lecm/base/title-with-filter-label</url>
			<properties>
				<title>page.user-profile.applet-settings.title</title>
				<filteredLabel>page.user-profile.filteredLabel</filteredLabel>
			</properties>
		</component>
		<!-- Menu -->
		<component>
			<region-id>menu</region-id>
			<url>/ru/it/lecm/user-profile/menu</url>
			<properties>
				<selected>userAppletSettings</selected>
			</properties>
		</component>
		<component>
			<region-id>applet-settings</region-id>
			<url>/lecm/signed/docflow/applet-settings</url>
		</component>
	</components>
</template-instance>
