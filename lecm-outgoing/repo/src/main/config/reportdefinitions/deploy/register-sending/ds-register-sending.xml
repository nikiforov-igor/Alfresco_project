                <item key="RU_RU"><![CDATA[Автономный округ]]></item>
            </l18>
        </field>
        <field alfrescoType="d:text" displayName="Область"
            javaValueClass="java.lang.String" jrFldName="region" queryFldName="{lecm-outgoing:recipient-assoc/lecm-representative:region}">
            <l18>
                <item key="RU_RU"><![CDATA[Область]]></item>
            </l18>
        </field>
        <field alfrescoType="d:text" displayName="Район"
            javaValueClass="java.lang.String" jrFldName="area" queryFldName="{lecm-outgoing:recipient-assoc/lecm-representative:area}">
