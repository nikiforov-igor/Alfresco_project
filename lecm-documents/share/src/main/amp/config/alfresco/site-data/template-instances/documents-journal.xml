<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
    <template-type>ru/it/lecm/documents/documents-journal</template-type>
    <components>
        <!-- Title -->
        <component>
            <region-id>title</region-id>
            <url>/lecm/base/title-with-filter-label</url>
            <properties>
                <title>page.lecm.documents-journal.title</title>
                <filteredLabel>page.lecm.documents-journal.filter.label</filteredLabel>
            </properties>
        </component>
        <!-- Toolbar -->
        <component>
            <region-id>toolbar</region-id>
            <url>/lecm/documents-journal/toolbar</url>
        </component>
        <!-- Menu -->
        <component>
            <region-id>menu</region-id>
            <url>/lecm/documents-journal/menu</url>
            <properties>
                <selected>contracts</selected>
            </properties>
        </component>
        <!-- Datagrid -->
        <component>
            <region-id>documents-grid</region-id>
            <url>/lecm/documents-journal/datagrid</url>
            <properties>
                <pagination>true</pagination>
                <bubblingLabel>documents</bubblingLabel>
            </properties>
        </component>
    </components>
</template-instance>
