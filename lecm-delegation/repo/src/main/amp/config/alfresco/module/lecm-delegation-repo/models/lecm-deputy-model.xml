<?xml version="1.0" encoding="UTF-8"?>
<model name="lecm-deputy:deputy-model" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>Deputy model</description>
	<author>ikhalikov</author>
	<version>1.0</version>

	<imports>
		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
		<!-- Импорт модели данных оргструктуры -->
		<import uri="http://www.it.ru/lecm/org/structure/1.0" prefix="lecm-orgstr"/>
		<import uri="http://www.it.ru/lecm/dictionary/1.0" prefix="lecm-dic"/>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.it.ru/lecm/deputy/1.0" prefix="lecm-deputy"/>
	</namespaces>

	<types>


		<type name="lecm-deputy:deputy-settings">
			<parent>cm:cmobject</parent>

			<associations>
				<association name="lecm-deputy:dictionary-assoc">
					<source>
						<mandatory>false</mandatory>
						<many>false</many>
					</source>
					<target>
						<class>lecm-dic:dictionary</class>
						<mandatory>false</mandatory>
						<many>true</many>
					</target>
				</association>
			</associations>
		</type>

		<type name="lecm-deputy:deputy">
			<parent>cm:cmobject</parent>

			<properties>
				<property name="lecm-deputy:complete-deputy-flag">
					<type>d:boolean</type>
					<default>true</default>
				</property>

				<property name="lecm-deputy:employee-assoc-ref">
					<type>d:text</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>

				<property name="lecm-deputy:employee-assoc-text-content">
					<type>d:text</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>

				<property name="lecm-deputy:subject-assoc-ref">
					<type>d:text</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>

				<property name="lecm-deputy:subject-assoc-text-content">
					<type>d:text</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>
			</properties>

			<associations>
				<association name="lecm-deputy:employee-assoc">
					<source>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>lecm-orgstr:employee</class>
						<mandatory>false</mandatory>
						<many>false</many>
					</target>
				</association>

				<association name="lecm-deputy:subject-assoc">
					<source>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>cm:cmobject</class>
						<mandatory>false</mandatory>
						<many>true</many>
					</target>
				</association>
			</associations>
		</type>
	</types>

	<aspects>
		<aspect name="lecm-deputy:has-deputy-aspect">
			<properties>
				<property name="lecm-deputy:deputy-assoc-ref">
					<type>d:text</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>

				<property name="lecm-deputy:deputy-assoc-text-content">
					<type>d:text</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>

				<property name="lecm-deputy:chief-assoc-ref">
					<type>d:text</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>

				<property name="lecm-deputy:chief-assoc-text-content">
					<type>d:text</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>
			</properties>
			<associations>
				<association name="lecm-deputy:deputy-assoc">
					<source>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>lecm-deputy:deputy</class>
						<mandatory>false</mandatory>
						<many>true</many>
					</target>
				</association>
			</associations>
		</aspect>
	</aspects>

</model>
