<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="webscript.ru.it.lecm.workflow.GetDocumentDataByTaskIdOrNodeRef.get"
		  class="ru.it.lecm.workflow.webscripts.GetDocumentDataByTaskIdOrNodeRef" parent="webscript"
		  p:workflowService-ref="WorkflowService"
		  p:nodeService-ref="nodeService"
		  p:workflowFoldersService-ref="lecm.workflow.folders.bootstrap" />

	<bean id="webscript.ru.it.lecm.workflow.SetAssigneesListConcurrencyWebscript.post"
		  class="ru.it.lecm.workflow.webscripts.SetAssigneesListConcurrencyWebscript" parent="webscript"
		  p:workflowAssigneesListService-ref="workflowAssigneesListService"/>

	<bean id="webscript.ru.it.lecm.workflow.SetDaysToComplete.post"
		  class="ru.it.lecm.workflow.webscripts.SetDaysToCompleteWebscript" parent="webscript"
		  p:workflowAssigneesListService-ref="workflowAssigneesListService"/>

</beans>
