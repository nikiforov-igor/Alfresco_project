<?xml version="1.0" encoding="UTF-8"?>
<model name="lecm-links:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>Logic ECM links model</description>
	<author>Logic ECM</author>
	<version>1.0</version>
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
	</imports>
	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/links/1.0" prefix="lecm-links"/>
	</namespaces>

	<types>
		<type name="lecm-links:link">
			<title>Logic Ecm Link</title>
			<parent>cm:content</parent>
			<properties>
				<!-- URL -->
				<property name="lecm-links:url">
					<type>d:text</type>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
			</properties>
		</type>
	</types>
</model>