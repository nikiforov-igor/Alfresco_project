<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>ru/it/lecm/wcalendar/absence</template-type>
	<properties>
		<pageFamily>wcalendar</pageFamily>
		<container>wcalendar</container>
	</properties>
	<components>
		<!-- Title -->
		<component>
			<region-id>title</region-id>
			<url>/lecm/base/title-with-filter-label</url>
			<properties>
				<title>page.absence.title</title>
				<filteredLabel>page.wcalendar.filteredLabel</filteredLabel>
			</properties>
		</component>
		<!-- Toolbar -->
		<component>
			<region-id>toolbar</region-id>
			<url>/ru/it/lecm/wcalendar/absence/toolbar</url>
		</component>
		<!-- Menu -->
		<component>
			<region-id>menu</region-id>
			<url>/ru/it/lecm/wcalendar/menu</url>
			<properties>
				<selected>wcalendarAbsence</selected>
			</properties>
		</component>
		<!-- Grid region -->
		<component>
			<region-id>absence</region-id>
			<url>/ru/it/lecm/wcalendar/absence/absence</url>
		</component>
		<component>
			<region-id>absence-summary-table</region-id>
			<url>/ru/it/lecm/wcalendar/absence/absence-summary-table</url>
		</component>
		<component>
			<region-id>absence-summary-table-wrapper</region-id>
			<url>/ru/it/lecm/wcalendar/absence/absence-summary-table-wrapper</url>
		</component>
		<component>
			<region-id>forbidden</region-id>
			<url>/lecm/base/forbidden</url>
		</component>
	</components>
</template-instance>
