<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xsi:schemaLocation="
               http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
            http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

    <import resource="classpath:META-INF/cxf/cxf.xml" />
    <import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
    <import resource="classpath:META-INF/cxf/cxf-servlet.xml" />

    <jaxws:endpoint address="/LECMStaffService" implementor="#staffService"
                    implementorClass="staffManager.WSStaffManagerPort"/>

    <bean id="staffService" class="staffManager.WSStaffManagerPort">
        <property name="objectFactory" ref="objectFactory"/>
    </bean>

    <bean id="objectFactory" class="staffManager.ObjectFactory" />

    <jaxws:endpoint address="/DocumentManagerImplService" implementor="#documentManagerServicePort"
                    implementorClass="documentManager.DocumentManagerImplServicePort"/>

    <bean id="documentManagerServicePort" class="documentManager.DocumentManagerImplServicePort"/>
</beans>