<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
    <template-type>ru/it/lecm/subscriptions/subscriptions-to-type</template-type>
    <components>
        <!-- Title -->
        <component>
            <region-id>title</region-id>
            <url>/lecm/base/title-with-filter-label</url>
            <properties>
                <title>page.subscriptions-to-type</title>
                <filteredLabel>page.subscriptions.filter.label</filteredLabel>
            </properties>
        </component>
        <!-- Toolbar -->
        <component>
            <region-id>toolbar</region-id>
            <url>/lecm/subscriptions/toolbar</url>
            <properties>
                <showButtons>true</showButtons>
                <showSearchBlock>true</showSearchBlock>
                <showExSearchBtn>true</showExSearchBtn>
            </properties>
        </component>
        <!-- Menu -->
        <component>
            <region-id>menu</region-id>
            <url>/lecm/subscriptions/menu</url>
            <properties>
                <selected>type</selected>
            </properties>
        </component>
        <!-- Datagrid -->
        <component>
            <region-id>type-grid</region-id>
            <url>/lecm/subscriptions/subscriptions-type-datagrid</url>
        </component>
    </components>
</template-instance>
