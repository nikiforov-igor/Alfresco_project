<?xml version="1.0" encoding="UTF-8"?>
<model name="lecm-exp:expertsModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">
    <description>Get experts Model</description>
    <author>DBashmakov</author>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
    </imports>
    <namespaces>
        <namespace uri="http://www.it.ru/logicECM/experts/1.0" prefix="lecm-exp"/>
    </namespaces>
    <aspects>
        <aspect name="lecm-exp:hasExperts">
            <associations>
                <association name="lecm-exp:experts">
                    <title>Experts List</title>
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>cm:person</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
        </aspect>
    </aspects>
</model>