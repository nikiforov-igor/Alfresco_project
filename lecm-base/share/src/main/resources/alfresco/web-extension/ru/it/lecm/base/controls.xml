        <forms-info>
    		<form-type id="create-form" evaluatorType="model-type" localName="Форма создания" />
    		<form-type id="edit-form" evaluatorType="node-type" localName="Форма редактирования" />
    		<form-layout id="one-column" localName="В одну колонку" template="" />
    		<form-layout id="two-columns" localName="В две колонки" template="" />
    		<form-layout id="with-tabs" localName="С вкладками" template="" />
