<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>ru/it/lecm/wcalendar/schedule</template-type>
	<properties>
		<pageFamily>wcalendar</pageFamily>
		<container>wcalendar</container>
	</properties>
	<components>
		<!-- Title -->
		<component>
			<region-id>title</region-id>
			<url>/lecm/base/title-with-filter-label</url>
			<properties>
				<title>page.schedule.title</title>
				<filteredLabel>page.wcalendar.filteredLabel</filteredLabel>
			</properties>
		</component>
		<!-- Toolbar -->
		<component>
			<region-id>toolbar</region-id>
			<url>/ru/it/lecm/wcalendar/schedule/toolbar</url>
			<properties>
				<bubblingLabel>scheduleToolbar</bubblingLabel>
			</properties>
		</component>
		<!-- Menu -->
		<component>
			<region-id>menu</region-id>
			<url>/ru/it/lecm/wcalendar/menu</url>
			<properties>
				<selected>wcalendarSchedule</selected>
			</properties>
		</component>
		<!-- Grid region -->
		<component>
			<region-id>schedule</region-id>
			<url>/ru/it/lecm/wcalendar/schedule/schedule</url>
			<properties>
				<bubblingLabel>scheduleDatagrid</bubblingLabel>
			</properties>
		</component>
		<component>
			<region-id>forbidden</region-id>
			<url>/lecm/base/forbidden</url>
		</component>
	</components>
</template-instance>
