							<control-param name="options">lecm-dic:plane_dictionary_values|PLAIN DICTIONARY,lecm-dic:hierarchical_dictionary_values|HIERARCHICAL DICTIONARY</control-param>
						</control>
					</field>
					<field id="cm:lecmModelAttributes" label-id="lecm.meditor.lbl.attrs">
						<control template="/ru/it/lecm/controls/attributes-datatable.ftl"/>
					</field>
					<field id="cm:lecmModelAssociations" label-id="lecm.meditor.lbl.assocs">
						<control template="/ru/it/lecm/controls/associations-datatable.ftl"/>
					</field>
				</appearance>
