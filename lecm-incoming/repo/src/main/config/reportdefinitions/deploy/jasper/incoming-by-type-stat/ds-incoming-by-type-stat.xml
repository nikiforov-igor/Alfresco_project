<?xml version="1.0" encoding="UTF-8"?>
<ds.config>
    <report.descriptor mnem="incoming-by-type-stat">
        <l18>
            <item key="RU_RU" value="Сводка по видам документов"/>
        </l18>
        <type mnem="JASPER">
            <l18>
                <item key="RU_RU" value="Jasper"/>
            </l18>
        </type>
        <provider
            javaClass="ru.it.lecm.reports.generators.GenericDSProviderBase" mnem="PROV_GENERIC_PROVIDER">
            <l18>
                <item key="RU_RU" value="genericProvider"/>
            </l18>
        </provider>
        <template filename="incoming-by-type-stat.jrxml" mnem="incoming-by-type-stat.jrxml">
            <l18>
                <item key="RU_RU" value="incoming-by-type-stat.jrxml"/>
            </l18>
        </template>
        <datasource.descriptor>
            <l18>
                <item key="RU_RU" value="ecb7c9b7-ea04-4641-a290-650442e86ee0"/>
            </l18>
        </datasource.descriptor>
    </report.descriptor>
    <query.descriptor>
        <offset>0</offset>
        <limit>-1</limit>
        <pgsize>-1</pgsize>
        <queryText><![CDATA[+TYPE:"lecm-incoming:document"]]></queryText>
        <querySort>col_DocType|ASC</querySort>
        <allVersions>true</allVersions>
        <preferedType>lecm-incoming:document</preferedType>
        <isMultiRow>true</isMultiRow>
        <isCustom>false</isCustom>
        <flags/>
    </query.descriptor>
    <fields>
        <field jrFldName="dateReg" order="0" queryFldName="{lecm-document-aspects:document-reg-attributes-assoc/lecm-document-aspects:reg-data-number}" displayName="Дата регистрации"
               javaValueClass="java.util.Date" alfrescoType="d:date">
            <parameter mnem="PARAM_TYPE_RANGE" paramType="PARAM_TYPE_RANGE">
                <l18>
                    <item key="RU_RU" value="Диапазон"/>
                </l18>
                <label1>
                    <item key="RU_RU" value="С"/>
                </label1>
                <label2>
                    <item key="RU_RU" value="По"/>
                </label2>
            </parameter>
        </field>

        <field jrFldName="incomingSubject" order="1" queryFldName="{lecm-incoming:subject-assoc-ref}" displayName="Тематика входящего"
               javaValueClass="java.lang.String" alfrescoType="lecm-doc-dic:subjects">
            <parameter mnem="PARAM_TYPE_LIST" paramType="PARAM_TYPE_LIST">
                <l18> <item key="RU_RU" value="Список"/> </l18>
            </parameter>
        </field>

        <field alfrescoType="d:boolean" order="2" displayName="Только на контроле"
               javaValueClass="java.lang.Boolean" jrFldName="incomingContractOnly" queryFldName="{lecm-incoming:is-on-control}">
            <l18>
                <item key="RU_RU" value="Только на контроле"/>
            </l18>
            <parameter mnem="PARAM_TYPE_VALUE" paramType="PARAM_TYPE_VALUE">
                <l18> <item key="RU_RU" value="Значение"/> </l18>
            </parameter>
        </field>

        <field alfrescoType="d:text" displayName="Вид документа"
            expression="{lecm-incoming:document-type-assoc/cm:name}" jrFldName="col_DocType">
            <l18>
                <item key="RU_RU" value="Вид документа"/>
            </l18>
        </field>
    </fields>
</ds.config>
