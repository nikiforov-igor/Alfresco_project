<?xml version="1.0" encoding="UTF-8"?>
<model name="lecmTermApproval:terminateApprovalWorkflow" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>Workflow model for parallel approvement</description>
	<author>pmelnikov</author>
	<version>1.0</version>

	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
		<import uri="http://www.alfresco.org/model/bpm/1.0" prefix="bpm"/>
		<import uri="http://www.it.ru/lecm/org/structure/1.0" prefix="lecm-orgstr"/>
		<import uri="http://www.it.ru/logicECM/statemachine/aspects/1.0" prefix="lecm-statemachine-aspects"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/model/approval/terminate/wokflow/1.0" prefix="lecmTermApproval"/>
	</namespaces>

	<types>
		<type name="lecmTermApproval:terminateApproval">
			<title>Workflow model for approvement terminate</title>
			<parent>bpm:startTask</parent>
			<mandatory-aspects>
				<aspect>lecm-statemachine-aspects:is-service-workflow</aspect>
			</mandatory-aspects>
		</type>
		<type name="lecmTermApproval:forceApproval">
			<title>Workflow model for approvement force</title>
			<parent>bpm:startTask</parent>
			<mandatory-aspects>
				<aspect>lecm-statemachine-aspects:is-service-workflow</aspect>
			</mandatory-aspects>
		</type>
	</types>
</model>
