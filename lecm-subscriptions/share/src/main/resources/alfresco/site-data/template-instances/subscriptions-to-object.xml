<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>ru/it/lecm/subscriptions/subscriptions-to-object</template-type>
	<components>
		<!-- Title -->
		<component>
			<region-id>title</region-id>
			<url>/lecm/base/title-with-filter-label</url>
			<properties>
				<title>page.subscriptions-to-object</title>
				<filteredLabel>page.subscriptions.filter.label</filteredLabel>
			</properties>
		</component>
		<!-- Toolbar -->
		<component>
			<region-id>toolbar</region-id>
			<url>/lecm/subscriptions/toolbar</url>
			<properties>
				<showButtons>true</showButtons>
				<showSearchBlock>true</showSearchBlock>
				<showExSearchBtn>true</showExSearchBtn>
				<showButtonsCreate>false</showButtonsCreate>
				<bubblingLabel>subscription-to-object</bubblingLabel>
			</properties>
		</component>
		<!-- Menu -->
		<component>
			<region-id>menu</region-id>
			<url>/lecm/subscriptions/menu</url>
			<properties>
				<selected>object</selected>
			</properties>
		</component>
		<!--Grid-->
		<component>
			<region-id>to-object-grid</region-id>
			<url>/lecm/subscriptions/to-object-datagrid</url>
			<properties>
				<pagination>true</pagination>
				<bubblingLabel>subscription-to-object</bubblingLabel>
				<advSearchFormId>subscription-to-object-search</advSearchFormId>
			</properties>
		</component>
		<component>
			<region-id>forbidden</region-id>
			<url>/lecm/base/forbidden</url>
		</component>
	</components>
</template-instance>
