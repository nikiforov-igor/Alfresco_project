					</field>
					<field id="cm:lecmTypeName">
						<control template="/ru/it/lecm/base-share/components/controls/hidden.ftl"/>
					</field>
					<field id="cm:lecmTypeTitle" label-id="lecm.meditor.lbl.dict.desc">
