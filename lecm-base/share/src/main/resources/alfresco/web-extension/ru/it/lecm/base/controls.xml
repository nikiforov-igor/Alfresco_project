                    <param id="showCreateNewLink" localName="" mandatory="false" visible="false">false</param>
                    <param id="showCreateNewButton" localName="" mandatory="false" visible="false">false</param>
	            </control>
	        </field-type>

	        <field-type id="association:lecm-notf-type:notification-type" localName="Тип уведомления">
	            <control id="association:lecm-notf-type:notification-type-default-control" template="" localName="Выбор типа уведомления">
	            	<param id="startLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Справочники/lecm-dic:Типы_x0020_доставки_x0020_уведомлений</param>
