                    <target>
                        <class>lecm-orgstr:employee</class>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </target>
                </association>
				<!-- Присутствовали -->
				<association name="lecm-meetings:attended-assoc">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
