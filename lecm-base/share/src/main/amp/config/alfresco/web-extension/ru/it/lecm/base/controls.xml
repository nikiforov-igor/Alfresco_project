					<param id="showCreateNewLink" localName="Показывать ссылку создания нового" mandatory="false" visible="true">false</param>
					<param id="showCreateNewButton" localName="Показывать кнопку создания нового" mandatory="false" visible="true">false</param>
				</control>
			</field-type>

			<field-type id="lecm-busjournal:objectType" localName="Объект журнала">
				<control id="lecm-busjournal:objectType-default-control" template="/ru/it/lecm/base-share/components/controls/association-autocomplete-control.ftl" localName="Выбор объекта журнала">
					<param id="startLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Справочники/lecm-dic:Тип_x0020_объекта</param>
					<param id="nameSubstituteString" localName="" mandatory="false" visible="false">{cm:name}</param>
					<param id="showCreateNewLink" localName="Показывать ссылку создания нового" mandatory="false" visible="true">false</param>
