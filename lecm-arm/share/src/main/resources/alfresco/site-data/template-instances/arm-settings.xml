<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>ru/it/lecm/arm/arm-settings</template-type>
	<components>
		<component>
			<region-id>tree</region-id>
			<url>/lecm/dictionary/tree</url>
			<properties>
				<dictionaryName>Настройки АРМ</dictionaryName>
			</properties>
		</component>
		<!-- Title -->
		<component>
			<region-id>title</region-id>
			<url>/lecm/base/title-with-filter-label</url>
			<properties>
				<title>page.title.arm.settings</title>
				<filteredLabel>page.content.filter.label</filteredLabel>
			</properties>
		</component>
		<!-- Toolbar -->
		<component>
			<region-id>toolbar</region-id>
			<url>/lecm/arm/toolbar</url>
			<properties>
				<pagination>true</pagination>
				<dictionaryName>Настройки АРМ</dictionaryName>
			</properties>
		</component>
		<!-- Edit -->
		<component>
			<region-id>main-form</region-id>
			<url>/lecm/arm/editNode</url>
			<properties>
				<plane>false</plane>
				<dictionaryName>Настройки АРМ</dictionaryName>
			</properties>
		</component>
	</components>
</template-instance>