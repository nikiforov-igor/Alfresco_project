<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="workflow.extension" parent="baseScript"
		  class="ru.it.lecm.workflow.extensions.WorkflowServiceJavascriptExtension"
		  p:extensionName="lecmWorkflowService"
		  p:workflowAssigneesListService-ref="workflowAssigneesListService"
		  p:workflowFoldersService-ref="lecm.workflow.folders.bootstrap"
		  p:orgstructureService-ref="serviceOrgstructure"
		  p:nodeService-ref="nodeService"/>

	<bean id="route.workflow.extension" parent="baseScript"
		  class="ru.it.lecm.workflow.extensions.RouteWorkflowJavascriptExtension"
		  p:extensionName="routeWorkflow"
		  p:nodeService-ref="nodeService"
		  p:workflowRunnerService-ref="&amp;workflowRunnerService"/> <!-- подключаем саму фабрику а не ее продукт -->

	<bean id="workflow.resultList.extension" parent="baseScript"
		  class="ru.it.lecm.workflow.extensions.WorkflowResultListJavascriptExtension"
		  p:workflowResultListService-ref="workflowResultListService"
		  p:namespaceService-ref="namespaceService"
		  p:extensionName="workflowResultList"/>

</beans>
