<alfresco-config>
    <config>
    	<field-types>
	        <field-type id="datetime" localName="Дата и время">
	            <control id="datetime-control" template="/ru/it/lecm/base-share/components/controls/date.ftl" localName="Выбор даты и времени">
	                <param id="showTime" localName="Показывать время" mandatory="false" visible="true">true</param>
	            </control>
	        </field-type>

	        <field-type id="date-range" localName="Временной промежуток">
	            <control id="date-range-default-control" template="/org/alfresco/components/form/controls/daterange.ftl" localName="Выбор промежутка времени"></control>
	        </field-type>
            <field-type id="int-range" localName="Числовой промежуток (int)">
	            <control id="int-range-default-control" template="/org/alfresco/components/form/controls/numberrange.ftl" localName="Выбор числового промежутка (int)"></control>
	        </field-type>

	        <field-type id="float-range" localName="Числовой промежуток (float)">
	            <control id="float-range-default-control" template="/org/alfresco/components/form/controls/numberrange.ftl" localName="Выбор числового промежутка (float)"></control>
	        </field-type>

	        <field-type id="double-range" localName="Числовой промежуток (double)">
	            <control id="double-range-default-control" template="/org/alfresco/components/form/controls/numberrange.ftl" localName="Выбор числового промежутка (double)"></control>
	        </field-type>

	        <field-type id="long-range" localName="Числовой промежуток (long)">
	            <control id="long-range-default-control" template="/org/alfresco/components/form/controls/numberrange.ftl" localName="Выбор числового промежутка (long)"></control>
	        </field-type>

