<?xml version="1.0" encoding="UTF-8"?>

<model name="lecm-notf-settings:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>Notification settings</description>
	<author>AIvkin</author>
	<version>1.0</version>

	<imports>
		<import uri="http://www.it.ru/lecm/notifications/types/1.0" prefix="lecm-notf-type"/>
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.it.ru/lecm/notifications/settings/1.0" prefix="lecm-notf-settings"/>
	</namespaces>

	<types>
		<type name="lecm-notf-settings:user">
			<title>Notifications user settings</title>
			<parent>cm:cmobject</parent>
			<associations>
				<!-- Ссылка на каналы доставки-->
				<association name="lecm-notf-settings:default-types-assoc">
					<source>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>lecm-notf-type:notification-type</class>
						<mandatory>false</mandatory>
						<many>true</many>
					</target>
				</association>
			</associations>
		</type>
	</types>
</model>