                <item key="RU_RU"><![CDATA[Улица]]></item>
            </l18>
        </field>
        <field alfrescoType="d:text" displayName="Дом"
            javaValueClass="java.lang.String" jrFldName="house" queryFldName="{lecm-outgoing:recipient-assoc/lecm-representative:house-number}">
