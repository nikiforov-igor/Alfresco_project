<?xml version="1.0" encoding="UTF-8"?>

<model name="lecm-notf-active-channel:dictionaryNotificationActiveChannel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>Notifications active channel</description>
	<author>AIvkin</author>
	<published>2013-01-17</published>
	<version>1.0</version>

	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
		<import uri="http://www.it.ru/lecm/notifications/1.0" prefix="lecm-notf"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.it.ru/lecm/notifications/channel/active/1.0" prefix="lecm-notf-active-channel"/>
	</namespaces>

	<types>
		<type name="lecm-notf-active-channel:notification">
			<title>Notification active channel</title>
			<parent>lecm-notf:notification-unit-base</parent>
			<properties>
				<property name="lecm-notf-active-channel:read-date">
					<title>Read date</title>
					<type>d:datetime</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
				<property name="lecm-notf-active-channel:is_read">
					<title>Is read</title>
					<type>d:boolean</type>
					<mandatory>true</mandatory>
					<default>false</default>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
			</properties>
		</type>
	</types>
</model>