<?xml version="1.0" encoding="UTF-8"?>
<model name="lecmWorkflowDocument:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">
    <description>Logic ECM document model</description>
    <author>Logic ECM</author>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/bpm/1.0" prefix="bpm"/>
        <import uri="http://www.it.ru/logicECM/statemachine/aspects/1.0" prefix="lecm-statemachine-aspects"/>    </imports>
    <namespaces>
        <namespace uri="http://www.it.ru/logicECM/document/workflow/1.0" prefix="lecmWorkflowDocument"/>
    </namespaces>
    <types>
        <type name="lecmWorkflowDocument:documentStamp">
            <parent>bpm:startTask</parent>
            <properties>
                <property name="lecmWorkflowDocument:documentStampSettings">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                </property>
            </properties>
            <mandatory-aspects>
                <aspect>lecm-statemachine-aspects:is-service-workflow</aspect>
            </mandatory-aspects>
        </type>
    </types>
    <aspects>
        <aspect name="lecmWorkflowDocument:createdNodeRefAspect">
            <properties>
                <property name="lecmWorkflowDocument:createdNodeRef">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                </property>
            </properties>
        </aspect>
    </aspects>

</model>