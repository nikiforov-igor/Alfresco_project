<?xml version="1.0" encoding="UTF-8"?>
<model name="lecm-doc-dic-type:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">
    <description>Logic ECM type settings dictionary model</description>
    <author>Logic ECM</author>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.it.ru/lecm/dictionary/1.0" prefix="lecm-dic"/>
    </imports>
    <namespaces>
        <namespace uri="http://www.it.ru/logicECM/document/dictionaries/typeSettings/1.0" prefix="lecm-doc-dic-type"/>
    </namespaces>

    <types>
        <type name="lecm-doc-dic-type:document-type-settings">
            <title>Logic Ecm Dictionary Document Type Settings</title>
            <parent>lecm-dic:plane_dictionary_values</parent>
            <properties>
                <property name="lecm-doc-dic-type:document-type">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
                <property name="lecm-doc-dic-type:attachment-category">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>both</tokenised>
                    </index>
                </property>
            </properties>
        </type>
    </types>
</model>