	            <control id="association:lecm-connect-types:connection-type-default-control" template="" localName="Выбор типа связи документа">
	            	<param id="startLocation" localName="" mandatory="false" visible="false">/app:company_home/cm:Business_x0020_platform/cm:LECM/cm:Сервис_x0020_Справочники/lecm-dic:Типы_x0020_связи</param>
