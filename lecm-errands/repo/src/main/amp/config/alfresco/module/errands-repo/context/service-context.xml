    <bean id="errandsStatusesFilter" parent="documentStatusesFilter">
        <property name="defaultFilters">
            <map>
                <entry key="lecm-errands:document" value="Все"/>
                <entry key="lecm-errands:document-archive" value="Неисполненные"/>
            </map>
        </property>
        <property name="filters">
            <map>
                <entry key="lecm-errands:document">
                    <map>
                        <entry key="Все" value="!Отменено, !Удалено, !Исполнено, !Не исполнено"/>
                        <entry key="Ожидает исполнения" value="Ожидает исполнения"/>
                        <entry key="Исполняется" value="Исполняется"/>
                        <entry key="На утверждении" value="На утверждении инициатором, На утверждении контролером"/>
                    </map>
                </entry>
            </map>
        </property>
        <property name="archiveFilters">
            <map>
                <entry key="lecm-errands:document">
                    <map>
                        <entry key="Неисполненные" value="Не исполнено"/>
                        <entry key="Исполненные" value="Исполнено"/>
                    </map>
                </entry>
            </map>
        </property>
    </bean>

    <bean id="errandsAccessPermissions" parent="documentsPermissions">
        <property name="permissions">
            <map>
                <entry key="lecm-errands:document">
                    <list>
                        <!-- Согласно 1433 - страница доступна всем пользователям-->
                    </list>
                </entry>
            </map>
        </property>
    </bean>

