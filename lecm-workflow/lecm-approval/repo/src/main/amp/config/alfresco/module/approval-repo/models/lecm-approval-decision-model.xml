<?xml version="1.0" encoding="UTF-8"?>
<model name="lecmApproveDecision:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>Workflow model for approval userTask decision</description>
	<author>Logic ECM</author>
	<version>1.0</version>

	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.it.ru/logicECM/model/approval/workflow/decision/1.0" prefix="lecmApproveDecision"/>
	</namespaces>

	<constraints>
		<constraint name="lecmApproveDecision:decisionConstraint" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value>NO_DECISION</value>
					<value>APPROVED</value>
					<value>REJECTED</value>
					<value>APPROVED_WITH_REMARK</value>
					<value>APPROVED_FORCED</value>
					<value>REJECTED_FORCED</value>
				</list>
			</parameter>
		</constraint>
	</constraints>
</model>
